  "type": "sigma",
  "status": "test",
  "created": "2023-09-15",
  "modified": "2024-03-05",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"OriginalFileName\": \"diskshadow.exe\"}, {\"Image|endswith\": \"\\\\diskshadow.exe\"}]\nselection_flag: {\"CommandLine|contains|windash\": \"-s \"}\nfilter_main_ext: {\"CommandLine|contains\": \".txt\"}",
    "condition": "all of selection_* and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "False postitve might occur with legitimate or uncommon extensions used internally. Initial baseline is required."
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
