  "references": [
    "https://github.com/SigmaHQ/sigma/pull/4467"
  ],
  "severity": "low",
  "type": "sigma",
  "status": "test",
  "created": "2023-10-10",
  "modified": "2024-02-26",
  "mitre": {
    "tactics": [
      "TA1059"
    ],
    "techniques": [
      "T003"
    ]
  },
  "detection": {
    "query": "selection_parent: {\"ParentImage|endswith\": \"\\\\ScreenConnect.ClientService.exe\"}\nselection_img: [{\"Image|endswith\": \"\\\\cmd.exe\"}, {\"OriginalFileName\": \"Cmd.Exe\"}]\nselection_cli: {\"CommandLine|contains\": \"\\\\TEMP\\\\ScreenConnect\\\\\"}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate use of ScreenConnect. Disable this rule if ScreenConnect is heavily used."
  ],
  "level": "low",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
