  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\DXCap.exe\"}, {\"OriginalFileName\": \"DXCap.exe\"}]\nselection_cli: {\"CommandLine|contains\": \" -c \"}",
    "condition": "all of selection*",
    "fields": []
  },
