  "type": "sigma",
  "status": "test",
  "created": "2022-06-03",
  "modified": "2022-06-03",
  "mitre": {
    "tactics": [
      "TA1059"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: {\"Image|endswith\": [\"python.exe\", \"python3.exe\", \"python2.exe\"]}\nselection_cli_1: {\"CommandLine|contains|all\": [\"import pty\", \".spawn(\"]}\nselection_cli_2: {\"CommandLine|contains\": \"from pty import spawn\"}",
    "condition": "selection_img and 1 of selection_cli_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
