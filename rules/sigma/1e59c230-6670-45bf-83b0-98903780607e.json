  "modified": "2023-06-14",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\gpscript.exe\"}, {\"OriginalFileName\": \"GPSCRIPT.EXE\"}]\nselection_cli: {\"CommandLine|contains\": [\" /logon\", \" /startup\"]}\nfilter_main_svchost: {\"ParentCommandLine\": \"C:\\\\windows\\\\system32\\\\svchost.exe -k netsvcs -p -s gpsvc\"}",
    "condition": "all of selection_* and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate uses of logon scripts distributed via group policy"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
