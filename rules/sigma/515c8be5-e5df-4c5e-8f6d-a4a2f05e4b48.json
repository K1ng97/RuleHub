  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\createdump.exe\"}, {\"OriginalFileName\": \"FX_VER_INTERNALNAME_STR\"}]\nselection_cli: {\"CommandLine|contains\": [\" -u \", \" --full \", \" -f \", \" --name \", \".dmp \"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Command lines that use the same flags"
  ],
  "level": "high",
  "rule_format": "standard",
