    "query": "selection_img: [{\"Image|endswith\": \"\\\\adplus.exe\"}, {\"OriginalFileName\": \"Adplus.exe\"}]\nselection_cli: {\"CommandLine|contains\": [\" -hang \", \" -pn \", \" -pmn \", \" -p \", \" -po \", \" -c \", \" -sc \"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate usage of Adplus for debugging purposes"
