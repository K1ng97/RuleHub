{
  "id": "3ffd6f51-e6c1-47b7-94b4-c1e61d4117c5",
  "name": "Certificate Exported Via Certutil.EXE",
  "description": "Detects the execution of the certutil with the \"exportPFX\" flag which allows the utility to export certificates.",
  "source": {
    "type": "sigma",
    "id": "3ffd6f51-e6c1-47b7-94b4-c1e61d4117c5",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_certutil_export_pfx.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_certutil_export_pfx.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1027"
  ],
  "author": "Florian Roth (Nextron Systems), Jonhnathan Ribeiro, oscd.community, Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://www.splunk.com/en_us/blog/security/a-golden-saml-journey-solarwinds-continued.html"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2023-02-15",
  "modified": "2024-03-05",
  "mitre": {
    "tactics": [
      "TA1027"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\certutil.exe\"}, {\"OriginalFileName\": \"CertUtil.exe\"}]\nselection_cli: {\"CommandLine|contains|windash\": \"-exportPFX \"}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "There legitimate reasons to export certificates. Investigate the activity to determine if it's benign"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}