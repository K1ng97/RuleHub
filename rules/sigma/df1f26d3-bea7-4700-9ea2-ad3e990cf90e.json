{
  "id": "df1f26d3-bea7-4700-9ea2-ad3e990cf90e",
  "name": "Node Process Executions",
  "description": "Detects the execution of other scripts using the Node executable packaged with Adobe Creative Cloud",
  "source": {
    "type": "sigma",
    "id": "df1f26d3-bea7-4700-9ea2-ad3e990cf90e",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_node_adobe_creative_cloud_abuse.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_node_adobe_creative_cloud_abuse.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1059007",
    "attackt1127"
  ],
  "author": "Max Altgelt (Nextron Systems)",
  "references": [
    "https://twitter.com/mttaggart/status/1511804863293784064"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-04-06",
  "modified": "2022-04-06",
  "mitre": {
    "tactics": [
      "TA1059",
      "TA1127"
    ],
    "techniques": [
      "T007"
    ]
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\Adobe Creative Cloud Experience\\\\libs\\\\node.exe\"}\nfilter: {\"CommandLine|contains\": \"Adobe Creative Cloud Experience\\\\js\"}",
    "condition": "selection and not filter",
    "fields": [
      "Image",
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}