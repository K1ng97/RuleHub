  "detection": {
    "query": "selection: {\"CommandLine|contains|all\": [\"verb:sync\", \"-source:RunCommand\", \"-dest:runCommand\"], \"Image|endswith\": \"\\\\msdeploy.exe\"}",
    "condition": "selection",
    "fields": [
      "ComputerName",
      "User",
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "System administrator Usage"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
