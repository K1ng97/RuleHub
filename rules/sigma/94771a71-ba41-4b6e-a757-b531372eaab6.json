{
  "id": "94771a71-ba41-4b6e-a757-b531372eaab6",
  "name": "File Download From Browser Process Via Inline URL",
  "description": "Detects execution of a browser process with a URL argument pointing to a file with a potentially interesting extension. This can be abused to download arbitrary files or to hide from the user for example by launching the browser in a minimized state.",
  "source": {
    "type": "sigma",
    "id": "94771a71-ba41-4b6e-a757-b531372eaab6",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_browsers_inline_file_download.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_browsers_inline_file_download.yml"
  },
  "tags": [
    "attackcommand-and-control",
    "attackt1105"
  ],
  "author": "Florian Roth (Nextron Systems), Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://twitter.com/mrd0x/status/1478116126005641220",
    "https://lolbas-project.github.io/lolbas/Binaries/Msedge/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-01-11",
  "modified": "2023-11-09",
  "mitre": {
    "tactics": [
      "TA1105"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: {\"Image|endswith\": [\"\\\\brave.exe\", \"\\\\chrome.exe\", \"\\\\msedge.exe\", \"\\\\opera.exe\", \"\\\\vivaldi.exe\"]}\nselection_http: {\"CommandLine|contains\": \"http\"}\nselection_extensions: {\"CommandLine|endswith\": [\".7z\", \".dat\", \".dll\", \".exe\", \".hta\", \".ps1\", \".psm1\", \".txt\", \".vbe\", \".vbs\", \".zip\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}