  "detection": {
    "query": "selection_img: [{\"Image|endswith\": [\"\\\\powershell.exe\", \"\\\\pwsh.exe\"]}, {\"OriginalFileName\": [\"PowerShell.EXE\", \"pwsh.dll\"]}, {\"Description\": \"Windows PowerShell\"}, {\"Product\": \"PowerShell Core 6\"}]\nselection_cli_xor: {\"CommandLine|contains\": \"bxor\"}\nselection_cli_other: {\"CommandLine|contains\": [\"ForEach\", \"for(\", \"for \", \"-join \", \"-join'\", \"-join\\\"\", \"-join`\", \"::Join\", \"[char]\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
