  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\dsacls.exe\"}, {\"OriginalFileName\": \"DSACLS.EXE\"}]\nselection_flag: {\"CommandLine|contains\": \" /G \"}\nselection_permissions: {\"CommandLine|contains\": [\"GR\", \"GE\", \"GW\", \"GA\", \"WP\", \"WD\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate administrators granting over permissive permissions to users"
  ],
  "level": "medium",
  "rule_format": "standard",
