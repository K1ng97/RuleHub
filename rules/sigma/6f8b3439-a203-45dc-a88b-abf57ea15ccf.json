    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_hktl_crackmapexec_powershell_obfuscation.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackexecution",
    "attackt1027005",
    "attackt1059001"
  ],
  "author": "Thomas Patzke",
  "references": [
    "https://github.com/byt3bl33d3r/CrackMapExec",
    "https://github.com/byt3bl33d3r/CrackMapExec/blob/0a49f75347b625e81ee6aa8c33d3970b5515ea9e/cme/helpers/powershell.py#L242"
  ],
  "severity": "high",
