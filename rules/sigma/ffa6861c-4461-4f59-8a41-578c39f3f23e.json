  "detection": {
    "query": "selection: [{\"CommandLine|contains\": [\"lsass.dmp\", \"lsass.zip\", \"lsass.rar\", \"Andrew.dmp\", \"Coredump.dmp\", \"NotLSASS.zip\", \"lsass_2\", \"lsassdump\", \"lsassdmp\"]}, {\"CommandLine|contains|all\": [\"lsass\", \".dmp\"]}, {\"CommandLine|contains|all\": [\"SQLDmpr\", \".mdmp\"]}, {\"CommandLine|contains|all\": [\"nanodump\", \".dmp\"]}]",
    "condition": "selection",
    "fields": []
