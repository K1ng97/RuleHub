  "detection": {
    "query": "selection_sql: {\"Image|endswith\": \"\\\\sqlcmd.exe\", \"CommandLine|contains|all\": [\"VeeamBackup\", \"From \"]}\nselection_db: {\"CommandLine|contains\": [\"BackupRepositories\", \"Backups\", \"Credentials\", \"HostCreds\", \"SmbFileShares\", \"Ssh_creds\", \"VSphereInfo\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
