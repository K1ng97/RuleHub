  "references": [
    "https://learn.microsoft.com/en-us/windows-server/administration/windows-commands/wbadmin-start-recovery",
    "https://lolbas-project.github.io/lolbas/Binaries/Wbadmin/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2024-05-10",
  "modified": "2024-05-10",
  "mitre": {
    "tactics": [
      "TA1490"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\wbadmin.exe\"}, {\"OriginalFileName\": \"WBADMIN.EXE\"}]\nselection_cli: {\"CommandLine|contains|all\": [\" recovery\", \"recoveryTarget\", \"itemtype:File\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
