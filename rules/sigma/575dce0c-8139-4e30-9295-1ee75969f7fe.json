  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-01-03",
  "modified": "2023-02-08",
  "mitre": {
    "tactics": [
      "TA1059",
      "TA1615"
    ],
    "techniques": [
      "T005"
    ]
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": [\"\\\\cscript.exe\", \"\\\\wscript.exe\"]}, {\"OriginalFileName\": [\"cscript.exe\", \"wscript.exe\"]}]\nselection_cli: {\"CommandLine|contains\": \"gatherNetworkInfo.vbs\"}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Administrative activity"
  ],
