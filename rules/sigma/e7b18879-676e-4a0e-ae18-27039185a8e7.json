  "detection": {
    "query": "selection_target: {\"TargetObject|contains\": \"\\\\SOFTWARE\\\\Microsoft\\\\NetSh\"}\nselection_folders_1: {\"Details|contains\": [\":\\\\Perflogs\\\\\", \":\\\\Users\\\\Public\\\\\", \":\\\\Windows\\\\Temp\\\\\", \"\\\\AppData\\\\Local\\\\Temp\\\\\", \"\\\\Temporary Internet\"]}\nselection_folders_2: [{\"Details|contains|all\": [\":\\\\Users\\\\\", \"\\\\Favorites\\\\\"]}, {\"Details|contains|all\": [\":\\\\Users\\\\\", \"\\\\Favourites\\\\\"]}, {\"Details|contains|all\": [\":\\\\Users\\\\\", \"\\\\Contacts\\\\\"]}, {\"Details|contains|all\": [\":\\\\Users\\\\\", \"\\\\Pictures\\\\\"]}]",
    "condition": "selection_target and 1 of selection_folders_*",
    "fields": []
  },
