    "file_path": "tmp/repos/sigma/rules/windows/image_load/image_load_side_load_mfdetours.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackprivilege-escalation",
    "attackt1574001"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "Internal Research"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2023-08-03",
  "modified": "2023-08-03",
  "mitre": {
    "tactics": [
      "TA1574"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"ImageLoaded|endswith\": \"\\\\mfdetours.dll\"}\nfilter_main_legit_path: {\"ImageLoaded|contains\": \":\\\\Program Files (x86)\\\\Windows Kits\\\\10\\\\bin\\\\\"}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
