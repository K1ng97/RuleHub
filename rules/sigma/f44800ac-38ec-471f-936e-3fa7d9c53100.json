  "detection": {
    "query": "selection1: {\"Image|endswith\": \"\\\\SepRemovalToolNative_x64.exe\"}\nselection2: {\"Image|endswith\": \"\\\\CATClean.exe\", \"CommandLine|contains\": \"--uninstall\"}\nselection3: {\"Image|endswith\": \"\\\\NetInstaller.exe\", \"CommandLine|contains\": \"-r\"}\nselection4: {\"Image|endswith\": \"\\\\WFPUnins.exe\", \"CommandLine|contains|all\": [\"/uninstall\", \"/enterprise\"]}",
    "condition": "1 of selection*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate administrative use (Should be investigated either way)"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
