  "mitre": {
    "tactics": [
      "TA1036"
    ],
    "techniques": [
      "T005"
    ]
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\svchost.exe\"}\nfilter_main_generic: {\"ParentImage|endswith\": [\"\\\\Mrt.exe\", \"\\\\MsMpEng.exe\", \"\\\\ngen.exe\", \"\\\\rpcnet.exe\", \"\\\\services.exe\", \"\\\\TiWorker.exe\"]}\nfilter_main_parent_null: {\"ParentImage\": null}\nfilter_main_parent_empty: {\"ParentImage\": [\"-\", \"\"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
