  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\regsvr32.exe\"}, {\"OriginalFileName\": \"REGSVR32.EXE\"}]\nselection_cli: {\"CommandLine|contains\": [\":\\\\ProgramData\\\\\", \":\\\\Temp\\\\\", \":\\\\Users\\\\Public\\\\\", \":\\\\Windows\\\\Temp\\\\\", \"\\\\AppData\\\\Local\\\\Temp\\\\\", \"\\\\AppData\\\\Roaming\\\\\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Some installers might execute \"regsvr32\" with DLLs located in %TEMP% or in %PROGRAMDATA%. Apply additional filters if necessary."
  ],
