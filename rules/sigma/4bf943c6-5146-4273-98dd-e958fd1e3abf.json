    "query": "selection: [{\"CommandLine|re\": \"\\\\$PSHome\\\\[\\\\s*\\\\d{1,3}\\\\s*\\\\]\\\\s*\\\\+\\\\s*\\\\$PSHome\\\\[\"}, {\"CommandLine|re\": \"\\\\$ShellId\\\\[\\\\s*\\\\d{1,3}\\\\s*\\\\]\\\\s*\\\\+\\\\s*\\\\$ShellId\\\\[\"}, {\"CommandLine|re\": \"\\\\$env:Public\\\\[\\\\s*\\\\d{1,3}\\\\s*\\\\]\\\\s*\\\\+\\\\s*\\\\$env:Public\\\\[\"}, {\"CommandLine|re\": \"\\\\$env:ComSpec\\\\[(\\\\s*\\\\d{1,3}\\\\s*,){2}\"}, {\"CommandLine|re\": \"\\\\*mdr\\\\*\\\\W\\\\s*\\\\)\\\\.Name\"}, {\"CommandLine|re\": \"\\\\$VerbosePreference\\\\.ToString\\\\(\"}, {\"CommandLine|re\": \"\\\\[String\\\\]\\\\s*\\\\$VerbosePreference\"}]",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
