    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_explorer_folder_shortcut_via_shell_binary.yml"
  },
  "tags": [
    "attackdiscovery",
    "attackt1135"
  ],
  "author": "@Kostastsale",
  "references": [
    "https://ss64.com/nt/shell.html"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "experimental",
  "created": "2022-12-22",
  "modified": "2024-08-23",
  "mitre": {
    "tactics": [
      "TA1135"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"ParentImage|endswith\": [\"\\\\cmd.exe\", \"\\\\powershell.exe\", \"\\\\pwsh.exe\"], \"Image|endswith\": \"\\\\explorer.exe\", \"CommandLine|contains\": \"shell:mycomputerfolder\"}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
