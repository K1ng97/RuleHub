{
  "id": "a746c9b8-a2fb-4ee5-a428-92bee9e99060",
  "name": "SQL Client Tools PowerShell Session Detection",
  "description": "This rule detects execution of a PowerShell code through the sqltoolsps.exe utility, which is included in the standard set of utilities supplied with the Microsoft SQL Server Management studio.\nScript blocks are not logged in this case, so this utility helps to bypass protection mechanisms based on the analysis of these logs.\n",
  "source": {
    "type": "sigma",
    "id": "a746c9b8-a2fb-4ee5-a428-92bee9e99060",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_mssql_sqltoolsps_susp_execution.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_mssql_sqltoolsps_susp_execution.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackexecution",
    "attackt1059001",
    "attackt1127"
  ],
  "author": "Agro (@agro_sev) oscd.communitly",
  "references": [
    "https://github.com/LOLBAS-Project/LOLBAS/blob/8283d8d91552213ded165fd36deb6cb9534cb443/yml/OtherMSBinaries/Sqltoolsps.yml",
    "https://twitter.com/pabraeken/status/993298228840992768"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2020-10-13",
  "modified": "2022-02-25",
  "mitre": {
    "tactics": [
      "TA1059",
      "TA1127"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: [{\"Image|endswith\": \"\\\\sqltoolsps.exe\"}, {\"ParentImage|endswith\": \"\\\\sqltoolsps.exe\"}, {\"OriginalFileName\": \"\\\\sqltoolsps.exe\"}]\nfilter: {\"ParentImage|endswith\": \"\\\\smss.exe\"}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "Direct PS command execution through SQLToolsPS.exe is uncommon, childprocess sqltoolsps.exe spawned by smss.exe is a legitimate action."
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}