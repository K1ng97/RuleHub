    "file_path": "tmp/repos/sigma/rules/windows/file/file_event/file_event_win_vscode_tunnel_renamed_execution.yml"
  },
  "tags": [
    "attackcommand-and-control"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://ipfyx.fr/post/visual-studio-code-tunnel/",
    "https://badoption.eu/blog/2023/01/31/code_c2.html"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2023-10-25",
  "modified": "2023-10-25",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"TargetFilename|endswith\": \"\\\\code_tunnel.json\"}\nfilter_main_legit_name: {\"Image|endswith\": [\"\\\\code-tunnel.exe\", \"\\\\code.exe\"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "file_event"
  ]
