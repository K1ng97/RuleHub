  "detection": {
    "query": "selection_main_img: [{\"Image|endswith\": \"\\\\whoami.exe\"}, {\"OriginalFileName\": \"whoami.exe\"}]\nselection_main_cli: {\"CommandLine|contains\": [\" /FO CSV\", \" -FO CSV\"]}\nselection_special: {\"CommandLine|contains\": \"whoami*>\"}",
    "condition": "all of selection_main_* or selection_special",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
