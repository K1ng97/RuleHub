  },
  "tags": [
    "attackdiscovery",
    "attackexecution",
    "attackt1543003"
  ],
  "author": "@gott_cyber",
  "references": [
    "https://learn.microsoft.com/en-us/windows-server/administration/windows-commands/dnscmd",
    "https://learn.microsoft.com/en-us/azure/dns/dns-zones-records",
    "https://lolbas-project.github.io/lolbas/Binaries/Dnscmd/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-07-31",
  "modified": "2023-02-04",
  "mitre": {
    "tactics": [
      "TA1543"
    ],
    "techniques": [
      "T003"
    ]
  },
  "detection": {
    "query": "selection_img: {\"Image|endswith\": \"\\\\dnscmd.exe\"}\nselection_cli: {\"CommandLine|contains\": [\"/enumrecords\", \"/enumzones\", \"/ZonePrint\", \"/info\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate administration use"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
