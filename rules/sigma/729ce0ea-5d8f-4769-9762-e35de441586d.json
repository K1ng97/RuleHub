    "query": "selection_binary: [{\"Image|endswith\": \"\\\\mpiexec.exe\"}, {\"Hashes|contains\": \"IMPHASH=d8b52ef6aaa3a81501bdfff9dbb96217\"}]\nselection_flags: {\"CommandLine|contains\": [\" /n 1 \", \" -n 1 \"]}",
    "condition": "all of selection*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
