  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\regedit.exe\"}, {\"OriginalFileName\": \"REGEDIT.EXE\"}]\nselection_cli: {\"CommandLine|contains\": [\" /i \", \".reg\"], \"CommandLine|re\": \":[^ \\\\\\\\]\"}\nfilter: {\"CommandLine|contains|windash\": [\" -e \", \" -a \", \" -c \"]}",
    "condition": "all of selection_* and not filter",
    "fields": [
      "ParentImage",
      "CommandLine"
    ]
  },
  "falsepositives": [
    "Unknown"
  ],
