{
  "id": "277a4393-446c-449a-b0ed-7fdc7795244c",
  "name": "Renamed FTP.EXE Execution",
  "description": "Detects the execution of a renamed \"ftp.exe\" binary based on the PE metadata fields",
  "source": {
    "type": "sigma",
    "id": "277a4393-446c-449a-b0ed-7fdc7795244c",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_renamed_ftp.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_renamed_ftp.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackexecution",
    "attackt1059",
    "attackt1202"
  ],
  "author": "Victor Sergeev, oscd.community",
  "references": [
    "https://lolbas-project.github.io/lolbas/Binaries/Ftp/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2020-10-09",
  "modified": "2023-02-03",
  "mitre": {
    "tactics": [
      "TA1059",
      "TA1202"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_original: {\"OriginalFileName\": \"ftp.exe\"}\nfilter_img: {\"Image|endswith\": \"\\\\ftp.exe\"}",
    "condition": "selection_original and not filter_img",
    "fields": [
      "CommandLine",
      "ParentImage"
    ]
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}