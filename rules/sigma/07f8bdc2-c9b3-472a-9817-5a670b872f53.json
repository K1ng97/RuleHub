  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\cmdkey.exe\"}, {\"OriginalFileName\": \"cmdkey.exe\"}]\nselection_cli: {\"CommandLine|contains|windash\": \" -l\"}",
    "condition": "all of selection*",
    "fields": [
      "CommandLine",
      "ParentCommandLine",
      "User"
    ]
  },
