      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"ParentImage|endswith\": \"\\\\provlaunch.exe\"}\nfilter_main_covered_children: [{\"Image|endswith\": [\"\\\\calc.exe\", \"\\\\cmd.exe\", \"\\\\cscript.exe\", \"\\\\mshta.exe\", \"\\\\notepad.exe\", \"\\\\powershell.exe\", \"\\\\pwsh.exe\", \"\\\\regsvr32.exe\", \"\\\\rundll32.exe\", \"\\\\wscript.exe\"]}, {\"Image|contains\": [\":\\\\PerfLogs\\\\\", \":\\\\Temp\\\\\", \":\\\\Users\\\\Public\\\\\", \"\\\\AppData\\\\Temp\\\\\", \"\\\\Windows\\\\System32\\\\Tasks\\\\\", \"\\\\Windows\\\\Tasks\\\\\", \"\\\\Windows\\\\Temp\\\\\"]}]",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}