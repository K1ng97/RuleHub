  "modified": "2024-03-13",
  "mitre": {
    "tactics": [
      "TA1012"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\regedit.exe\"}, {\"OriginalFileName\": \"REGEDIT.EXE\"}]\nselection_cli: {\"CommandLine|contains|windash\": \" -E \"}\nfilter_1: {\"CommandLine|contains\": [\"hklm\", \"hkey_local_machine\"]}\nfilter_2: {\"CommandLine|endswith\": [\"\\\\system\", \"\\\\sam\", \"\\\\security\"]}",
    "condition": "all of selection_* and not all of filter_*",
    "fields": [
      "ParentImage",
      "CommandLine"
    ]
  },
  "falsepositives": [
    "Legitimate export of keys"
  ],
  "level": "low",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}