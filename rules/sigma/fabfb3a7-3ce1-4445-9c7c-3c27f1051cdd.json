  },
  "tags": [
    "attackdefense-evasion",
    "attackexecution",
    "attackt1036"
  ],
  "author": "X__Junior (Nextron Systems)",
  "references": [
    "https://cocomelonc.github.io/malware/2022/11/02/malware-pers-18.html",
    "https://www.hexacorn.com/blog/2018/08/31/beyond-good-ol-run-key-part-85/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2023-06-30",
  "modified": "2023-06-30",
  "mitre": {
    "tactics": [
      "TA1036"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\WerFault.exe\"}, {\"OriginalFileName\": \"WerFault.exe\"}]\nselection_cli: {\"CommandLine|contains\": \" -pr \"}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
