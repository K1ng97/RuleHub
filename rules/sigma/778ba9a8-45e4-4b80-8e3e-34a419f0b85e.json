{
  "id": "778ba9a8-45e4-4b80-8e3e-34a419f0b85e",
  "name": "TeamViewer Domain Query By Non-TeamViewer Application",
  "description": "Detects DNS queries to a TeamViewer domain only resolved by a TeamViewer client by an image that isn't named TeamViewer (sometimes used by threat actors for obfuscation)",
  "source": {
    "type": "sigma",
    "id": "778ba9a8-45e4-4b80-8e3e-34a419f0b85e",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/dns_query_win_teamviewer_domain_query_by_uncommon_app.yml",
    "file_path": "tmp/repos/sigma/rules/windows/dns_query/dns_query_win_teamviewer_domain_query_by_uncommon_app.yml"
  },
  "tags": [
    "attackcommand-and-control",
    "attackt1219"
  ],
  "author": "Florian Roth (Nextron Systems)",
  "references": [
    "https://www.teamviewer.com/en-us/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-01-30",
  "modified": "2023-09-18",
  "mitre": {
    "tactics": [
      "TA1219"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"QueryName\": [\"taf.teamviewer.com\", \"udp.ping.teamviewer.com\"]}\nfilter_main_teamviewer: {\"Image|contains\": \"TeamViewer\"}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown binary names of TeamViewer",
    "Depending on the environment the rule might require some initial tuning before usage to avoid FP with third party applications"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "dns_query"
  ]
}