  "detection": {
    "query": "selection: {\"Image|endswith\": [\"\\\\wscript.exe\", \"\\\\cscript.exe\"], \"TargetFilename|startswith\": [\"C:\\\\Users\\\\\", \"C:\\\\ProgramData\"], \"TargetFilename|endswith\": [\".jse\", \".vbe\", \".js\", \".vba\", \".vbs\"]}",
    "condition": "selection",
    "fields": [
      "Image",
      "TargetFilename"
    ]
  },
  "falsepositives": [
