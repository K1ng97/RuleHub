{
  "id": "3b6ab547-8ec2-4991-b9d2-2b06702a48d7",
  "name": "PowerShell Download Pattern",
  "description": "Detects a Powershell process that contains download commands in its command line string",
  "source": {
    "type": "sigma",
    "id": "3b6ab547-8ec2-4991-b9d2-2b06702a48d7",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_powershell_download_patterns.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_powershell_download_patterns.yml"
  },
  "tags": [
    "attackexecution",
    "attackt1059001"
  ],
  "author": "Florian Roth (Nextron Systems), oscd.community, Jonhnathan Ribeiro",
  "references": [
    "https://blog.redteam.pl/2020/06/black-kingdom-ransomware.html",
    "https://lab52.io/blog/winter-vivern-all-summer/",
    "https://hatching.io/blog/powershell-analysis/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2019-01-16",
  "modified": "2023-01-26",
  "mitre": {
    "tactics": [
      "TA1059"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": [\"\\\\powershell.exe\", \"\\\\pwsh.exe\"]}, {\"OriginalFileName\": [\"PowerShell.EXE\", \"pwsh.dll\"]}]\nselection_cli: {\"CommandLine|contains|all\": [\"new-object\", \"net.webclient).\", \"download\"], \"CommandLine|contains\": [\"string(\", \"file(\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}