{
  "id": "576426ad-0131-4001-ae01-be175da0c108",
  "name": "PowerShell Script Dropped Via PowerShell.EXE",
  "description": "Detects PowerShell creating a PowerShell file (.ps1). While often times this behavior is benign, sometimes it can be a sign of a dropper script trying to achieve persistence.",
  "source": {
    "type": "sigma",
    "id": "576426ad-0131-4001-ae01-be175da0c108",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/file_event_win_powershell_drop_powershell.yml",
    "file_path": "tmp/repos/sigma/rules/windows/file/file_event/file_event_win_powershell_drop_powershell.yml"
  },
  "tags": [
    "attackpersistence"
  ],
  "author": "frack113",
  "references": [
    "https://www.zscaler.com/blogs/security-research/onenote-growing-threat-malware-distribution"
  ],
  "severity": "low",
  "type": "sigma",
  "status": "test",
  "created": "2023-05-09",
  "modified": "2023-05-09",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": [\"\\\\powershell.exe\", \"\\\\pwsh.exe\"], \"TargetFilename|endswith\": \".ps1\"}\nfilter_main_psscriptpolicytest: {\"TargetFilename|contains\": \"__PSScriptPolicyTest_\"}\nfilter_main_appdata: {\"TargetFilename|startswith\": \"C:\\\\Users\\\\\", \"TargetFilename|contains\": \"\\\\AppData\\\\Local\\\\Temp\\\\\"}\nfilter_main_windows_temp: {\"TargetFilename|startswith\": \"C:\\\\Windows\\\\Temp\\\\\"}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "False positives will differ depending on the environment and scripts used. Apply additional filters accordingly."
  ],
  "level": "low",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "file_event"
  ]
}