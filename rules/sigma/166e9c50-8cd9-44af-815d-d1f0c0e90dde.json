{
  "id": "166e9c50-8cd9-44af-815d-d1f0c0e90dde",
  "name": "Suspicious Svchost Process Access",
  "description": "Detects suspicious access to the \"svchost\" process such as that used by Invoke-Phantom to kill the thread of the Windows event logging service.",
  "source": {
    "type": "sigma",
    "id": "166e9c50-8cd9-44af-815d-d1f0c0e90dde",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_access_win_svchost_susp_access_request.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_access/proc_access_win_svchost_susp_access_request.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1562002"
  ],
  "author": "Tim Burrell",
  "references": [
    "https://github.com/hlldz/Invoke-Phant0m",
    "https://twitter.com/timbmsft/status/900724491076214784"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2020-01-02",
  "modified": "2023-01-30",
  "mitre": {
    "tactics": [
      "TA1562"
    ],
    "techniques": [
      "T002"
    ]
  },
  "detection": {
    "query": "selection: {\"TargetImage|endswith\": \":\\\\Windows\\\\System32\\\\svchost.exe\", \"GrantedAccess\": \"0x1F3FFF\", \"CallTrace|contains\": \"UNKNOWN\"}\nfilter_main_msbuild: {\"SourceImage|contains\": \":\\\\Program Files\\\\Microsoft Visual Studio\\\\\", \"SourceImage|endswith\": \"\\\\MSBuild\\\\Current\\\\Bin\\\\MSBuild.exe\", \"CallTrace|contains\": [\"Microsoft.Build.ni.dll\", \"System.ni.dll\"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_access"
  ]
}