  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\ntdsutil.exe\"}, {\"OriginalFileName\": \"ntdsutil.exe\"}]\nselection_cli: [{\"CommandLine|contains|all\": [\"snapshot\", \"mount \"]}, {\"CommandLine|contains|all\": [\"ac\", \" i\", \" ntds\"]}]",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate usage to restore snapshots",
    "Legitimate admin activity"
  ],
