  "modified": "2023-11-09",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\InstallUtil.exe\"}, {\"OriginalFileName\": \"InstallUtil.exe\"}]\nselection_cli: {\"CommandLine|contains\": [\"ftp://\", \"http://\", \"https://\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
