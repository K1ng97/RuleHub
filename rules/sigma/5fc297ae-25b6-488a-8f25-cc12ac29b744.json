    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_qemu_suspicious_execution.yml"
  },
  "tags": [
    "attackcommand-and-control",
    "attackt1090",
    "attackt1572"
  ],
  "author": "Muhammad Faisal (@faisalusuf), Hunter Juhan (@threatHNTR)",
  "references": [
    "https://securelist.com/network-tunneling-with-qemu/111803/",
    "https://www.qemu.org/docs/master/system/invocation.html#hxtool-5"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2024-06-03",
  "modified": "2024-06-03",
  "mitre": {
    "tactics": [
      "TA1090",
      "TA1572"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"CommandLine|contains\": [\"-m 1M\", \"-m 2M\", \"-m 3M\"], \"CommandLine|contains|all\": [\"restrict=off\", \"-netdev \", \"connect=\", \"-nographic\"]}\nfilter_main_normal_usecase: {\"CommandLine|contains\": [\" -cdrom \", \" type=virt \", \" -blockdev \"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
