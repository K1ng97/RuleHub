      "T001",
      "T004"
    ]
  },
  "detection": {
    "query": "selection_findstr: [{\"CommandLine|contains\": \"findstr\"}, {\"Image|endswith\": \"findstr.exe\"}, {\"OriginalFileName\": \"FINDSTR.EXE\"}]\nselection_cli_search_subfolder: {\"CommandLine|contains|windash\": \" -s \"}\nselection_cli_search_insensitive: {\"CommandLine|contains|windash\": \" -i \"}",
    "condition": "selection_findstr and all of selection_cli_search_*",
    "fields": []
  },
  "falsepositives": [
    "Administrative or software activity"
  ],
  "level": "low",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
