  "mitre": {
    "tactics": [
      "TA1059",
      "TA1105"
    ],
    "techniques": [
      "T001",
      "T003"
    ]
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\cmd.exe\", \"CommandLine|contains|all\": [\"http\", \"://\", \"%AppData%\"]}",
    "condition": "selection",
    "fields": [
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "High"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}