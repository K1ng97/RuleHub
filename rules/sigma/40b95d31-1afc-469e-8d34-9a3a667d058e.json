    "tactics": [
      "TA1072",
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": [\"\\\\csi.exe\", \"\\\\rcsi.exe\"]}, {\"OriginalFileName\": [\"csi.exe\", \"rcsi.exe\"]}]\nselection_cli: {\"Company\": \"Microsoft Corporation\"}",
    "condition": "all of selection*",
    "fields": [
      "ComputerName",
      "User",
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "Legitimate usage by software developers"
  ],
