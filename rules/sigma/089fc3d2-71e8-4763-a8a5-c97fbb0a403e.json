  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\regsvr32.exe\"}, {\"OriginalFileName\": \"REGSVR32.EXE\"}]\nselection_cli: {\"CommandLine|endswith\": [\".bin\", \".bmp\", \".cr2\", \".dat\", \".eps\", \".gif\", \".ico\", \".jpeg\", \".jpg\", \".nef\", \".orf\", \".png\", \".raw\", \".sr2\", \".temp\", \".tif\", \".tiff\", \".tmp\", \".rtf\", \".txt\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
