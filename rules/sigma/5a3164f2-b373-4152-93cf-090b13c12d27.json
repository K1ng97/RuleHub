{
  "id": "5a3164f2-b373-4152-93cf-090b13c12d27",
  "name": "Potentially Suspicious Child Process Of VsCode",
  "description": "Detects uncommon or suspicious child processes spawning from a VsCode \"code.exe\" process. This could indicate an attempt of persistence via VsCode tasks or terminal profiles.",
  "source": {
    "type": "sigma",
    "id": "5a3164f2-b373-4152-93cf-090b13c12d27",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_vscode_child_processes_anomalies.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_vscode_child_processes_anomalies.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackexecution",
    "attackt1202",
    "attackt1218"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://twitter.com/nas_bench/status/1618021838407495681",
    "https://twitter.com/nas_bench/status/1618021415852335105"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2023-01-26",
  "modified": "2023-10-25",
  "mitre": {
    "tactics": [
      "TA1202",
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_parent: {\"ParentImage|endswith\": \"\\\\code.exe\"}\nselection_children_images: {\"Image|endswith\": [\"\\\\calc.exe\", \"\\\\regsvr32.exe\", \"\\\\rundll32.exe\", \"\\\\cscript.exe\", \"\\\\wscript.exe\"]}\nselection_children_cli: {\"Image|endswith\": [\"\\\\powershell.exe\", \"\\\\pwsh.exe\", \"\\\\cmd.exe\"], \"CommandLine|contains\": [\"Invoke-Expressions\", \"IEX\", \"Invoke-Command\", \"ICM\", \"DownloadString\", \"rundll32\", \"regsvr32\", \"wscript\", \"cscript\"]}\nselection_children_paths: {\"Image|contains\": [\":\\\\Users\\\\Public\\\\\", \":\\\\Windows\\\\Temp\\\\\", \":\\\\Temp\\\\\"]}",
    "condition": "selection_parent and 1 of selection_children_*",
    "fields": []
  },
  "falsepositives": [
    "In development environment where VsCode is used heavily. False positives may occur when developers use task to compile or execute different types of code. Remove or add processes accordingly"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}