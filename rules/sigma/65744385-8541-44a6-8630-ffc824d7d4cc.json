  },
  "tags": [
    "attackcredential-access",
    "attackt1528"
  ],
  "author": "@SerkinValery",
  "references": [
    "https://www.bleepingcomputer.com/news/security/microsoft-teams-stores-auth-tokens-as-cleartext-in-windows-linux-macs/",
    "https://www.vectra.ai/blog/undermining-microsoft-teams-security-by-mining-tokens"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "experimental",
  "created": "2024-07-22",
  "modified": "2024-07-22",
  "mitre": {
    "tactics": [
      "TA1528"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"FileName|contains\": [\"\\\\Microsoft\\\\Teams\\\\Cookies\", \"\\\\Microsoft\\\\Teams\\\\Local Storage\\\\leveldb\"]}\nfilter_main_legit_location: {\"Image|endswith\": \"\\\\Microsoft\\\\Teams\\\\current\\\\Teams.exe\"}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "file_access"
  ]
}