    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_cmdl32_arbitrary_file_download.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackexecution",
    "attackt1202",
    "attackt1218"
  ],
  "author": "frack113",
  "references": [
    "https://lolbas-project.github.io/lolbas/Binaries/Cmdl32/",
    "https://twitter.com/SwiftOnSecurity/status/1455897435063074824",
    "https://github.com/LOLBAS-Project/LOLBAS/pull/151"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2021-11-03",
  "modified": "2024-04-22",
  "mitre": {
    "tactics": [
      "TA1202",
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\cmdl32.exe\"}, {\"OriginalFileName\": \"CMDL32.EXE\"}]\nselection_cli: {\"CommandLine|contains|all\": [\"/vpn\", \"/lan\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
