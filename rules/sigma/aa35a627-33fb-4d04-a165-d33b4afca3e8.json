  "detection": {
    "query": "selection: {\"TargetImage|endswith\": \"\\\\lsass.exe\", \"SourceImage|endswith\": \":\\\\Windows\\\\system32\\\\wsmprovhost.exe\"}\nfilter_main_access: {\"GrantedAccess\": \"0x80000000\"}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
