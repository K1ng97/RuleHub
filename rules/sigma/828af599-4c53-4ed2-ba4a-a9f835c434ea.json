  "name": "Fax Service DLL Search Order Hijack",
  "description": "The Fax service attempts to load ualapi.dll, which is non-existent. An attacker can then (side)load their own malicious DLL using this service.",
  "source": {
    "type": "sigma",
    "id": "828af599-4c53-4ed2-ba4a-a9f835c434ea",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/image_load_side_load_ualapi.yml",
    "file_path": "tmp/repos/sigma/rules/windows/image_load/image_load_side_load_ualapi.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackpersistence",
    "attackt1574001"
  ],
  "author": "NVISO",
  "references": [
    "https://windows-internals.com/faxing-your-way-to-system/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2020-05-04",
  "modified": "2022-06-02",
  "mitre": {
    "tactics": [
      "TA1574"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\fxssvc.exe\", \"ImageLoaded|endswith\": \"ualapi.dll\"}\nfilter: {\"ImageLoaded|startswith\": \"C:\\\\Windows\\\\WinSxS\\\\\"}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
