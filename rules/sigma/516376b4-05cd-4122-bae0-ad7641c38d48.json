{
  "id": "516376b4-05cd-4122-bae0-ad7641c38d48",
  "name": "Mailbox Export to Exchange Webserver",
  "description": "Detects a successful export of an Exchange mailbox to untypical directory or with aspx name suffix which can be used to place a webshell or the needed role assignment for it",
  "source": {
    "type": "sigma",
    "id": "516376b4-05cd-4122-bae0-ad7641c38d48",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/win_exchange_proxyshell_mailbox_export.yml",
    "file_path": "tmp/repos/sigma/rules/windows/builtin/msexchange/win_exchange_proxyshell_mailbox_export.yml"
  },
  "tags": [
    "attackpersistence",
    "attackt1505003"
  ],
  "author": "Florian Roth (Nextron Systems), Rich Warren, Christian Burkard (Nextron Systems)",
  "references": [
    "https://blog.orange.tw/2021/08/proxylogon-a-new-attack-surface-on-ms-exchange-part-1.html"
  ],
  "severity": "critical",
  "type": "sigma",
  "status": "test",
  "created": "2021-08-09",
  "modified": "2023-04-30",
  "mitre": {
    "tactics": [
      "TA1505"
    ],
    "techniques": [
      "T003"
    ]
  },
  "detection": {
    "query": "export_command: {\"|all\": [\"New-MailboxExportRequest\", \" -Mailbox \"]}\nexport_params: [\"-FilePath \\\"\\\\\\\\\\\\\\\\\", \".aspx\"]\nrole_assignment: {\"|all\": [\"New-ManagementRoleAssignment\", \" -Role \\\"Mailbox Import Export\\\"\", \" -User \"]}",
    "condition": "(export_command and export_params) or role_assignment",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
  "level": "critical",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "msexchange-management"
  ]
}