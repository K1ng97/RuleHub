  "detection": {
    "query": "selection: {\"PipeName|contains\": [\"\\\\pipe\\\\\", \"\\\\pipe\\\\srvsvc\"]}\nfilter_optional_ctx: {\"PipeName|contains\": \"\\\\CtxShare\"}\nfilter_optional_default: {\"PipeName|startswith\": \"\\\\pipe\\\\\"}",
    "condition": "selection and not 1 of filter_optional_*",
    "fields": []
  },
  "falsepositives": [
    "\\pipe\\LOCAL\\Monitorian"
