  },
  "detection": {
    "query": "selection_exec: {\"Image|endswith\": [\"\\\\wscript.exe\", \"\\\\cscript.exe\"]}\nselection_paths: {\"CommandLine|contains\": [\":\\\\Temp\\\\\", \":\\\\Tmp\\\\\", \":\\\\Users\\\\Public\\\\\", \":\\\\Windows\\\\Temp\\\\\", \"\\\\AppData\\\\Local\\\\Temp\\\\\"]}\nselection_ext: {\"CommandLine|contains\": [\".js\", \".jse\", \".vba\", \".vbe\", \".vbs\", \".wsf\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Some installers might generate a similar behavior. An initial baseline is required"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}