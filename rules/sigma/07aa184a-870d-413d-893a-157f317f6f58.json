  "detection": {
    "query": "selection: {\"CommandLine|contains\": \"gatherNetworkInfo.vbs\"}\nfilter: {\"Image|endswith\": [\"\\\\cscript.exe\", \"\\\\wscript.exe\"]}",
    "condition": "selection and not filter",
    "fields": []
  },
