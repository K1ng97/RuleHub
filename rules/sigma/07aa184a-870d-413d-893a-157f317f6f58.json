  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2023-02-08",
  "modified": "2023-02-08",
  "mitre": {
    "tactics": [
      "TA1059",
      "TA1615"
    ],
    "techniques": [
      "T005"
