  "detection": {
    "query": "test_3: [{\"Payload|contains\": [\"get-localgroup\", \"Get-LocalGroupMember\"]}, {\"ContextInfo|contains\": [\"get-localgroup\", \"Get-LocalGroupMember\"]}]\ntest_6: [{\"Payload|contains|all\": [\"Get-WMIObject\", \"Win32_Group\"]}, {\"ContextInfo|contains|all\": [\"Get-WMIObject\", \"Win32_Group\"]}]",
