  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\netsh.exe\"}, {\"OriginalFileName\": \"netsh.exe\"}]\nselection_cli_1: {\"CommandLine|contains|all\": [\"firewall\", \"set\", \"opmode\", \"disable\"]}\nselection_cli_2: {\"CommandLine|contains|all\": [\"advfirewall\", \"set\", \"state\", \"off\"]}",
    "condition": "selection_img and 1 of selection_cli_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate administration activity"
  ],
  "level": "medium",
  "rule_format": "standard",
