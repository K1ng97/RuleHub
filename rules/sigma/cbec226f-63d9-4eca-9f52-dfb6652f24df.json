  "mitre": {
    "tactics": [
      "TA1036"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"ParentImage|endswith\": [\"\\\\minesweeper.exe\", \"\\\\winver.exe\", \"\\\\bitsadmin.exe\"]}\nselection_special: {\"ParentImage|endswith\": [\"\\\\csrss.exe\", \"\\\\certutil.exe\", \"\\\\eventvwr.exe\", \"\\\\calc.exe\", \"\\\\notepad.exe\"]}\nfilter_special: {\"Image|endswith\": [\"\\\\WerFault.exe\", \"\\\\wermgr.exe\", \"\\\\conhost.exe\", \"\\\\mmc.exe\", \"\\\\win32calc.exe\", \"\\\\notepad.exe\"]}\nfilter_null: {\"Image\": null}",
    "condition": "selection or ( selection_special and not 1 of filter_* )",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}