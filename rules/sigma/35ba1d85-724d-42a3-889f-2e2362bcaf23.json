  },
  "detection": {
    "query": "selection: {\"EventID\": 4661, \"ObjectType\": [\"SAM_USER\", \"SAM_GROUP\"]}\nselection_object: [{\"ObjectName|endswith\": [\"-512\", \"-502\", \"-500\", \"-505\", \"-519\", \"-520\", \"-544\", \"-551\", \"-555\"]}, {\"ObjectName|contains\": \"admin\"}]\nfilter: {\"SubjectUserName|endswith\": \"$\"}",
    "condition": "selection and selection_object and not filter",
    "fields": []
  },
  "falsepositives": [
    "If source account name is not an admin then its super suspicious"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "security"
  ]
