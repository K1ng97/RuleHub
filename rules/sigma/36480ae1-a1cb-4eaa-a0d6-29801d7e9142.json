    "tactics": [
      "TA1036"
    ],
    "techniques": [
      "T003"
    ]
  },
  "detection": {
    "query": "selection: {\"OriginalFileName\": [\"Cmd.Exe\", \"CONHOST.EXE\", \"7z.exe\", \"WinRAR.exe\", \"wevtutil.exe\", \"net.exe\", \"net1.exe\", \"netsh.exe\", \"InstallUtil.exe\"]}\nfilter: {\"Image|endswith\": [\"\\\\cmd.exe\", \"\\\\conhost.exe\", \"\\\\7z.exe\", \"\\\\WinRAR.exe\", \"\\\\wevtutil.exe\", \"\\\\net.exe\", \"\\\\net1.exe\", \"\\\\netsh.exe\", \"\\\\InstallUtil.exe\"]}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "Custom applications use renamed binaries adding slight change to binary name. Typically this is easy to spot and add to whitelist"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}