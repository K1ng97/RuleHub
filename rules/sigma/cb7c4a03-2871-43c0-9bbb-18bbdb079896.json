  },
  "tags": [
    "attackdefense-evasion",
    "attackt1070005"
  ],
  "author": "oscd.community, @redcanary, Zach Stanford @svch0st",
  "references": [
    "https://github.com/redcanaryco/atomic-red-team/blob/f339e7da7d05f6057fdfcdd3742bfcf365fee2a9/atomics/T1070.005/T1070.005.md"
  ],
  "severity": "low",
  "type": "sigma",
  "status": "test",
  "created": "2020-10-08",
  "modified": "2023-02-21",
  "mitre": {
    "tactics": [
      "TA1070"
    ],
    "techniques": [
      "T005"
    ]
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": [\"\\\\net.exe\", \"\\\\net1.exe\"]}, {\"OriginalFileName\": [\"net.exe\", \"net1.exe\"]}]\nselection_cli: {\"CommandLine|contains|all\": [\"share\", \"/delete\"]}",
    "condition": "all of selection*",
    "fields": []
  },
  "falsepositives": [
    "Administrators or Power users may remove their shares via cmd line"
  ],
  "level": "low",
  "rule_format": "standard",
  "platforms": [],
