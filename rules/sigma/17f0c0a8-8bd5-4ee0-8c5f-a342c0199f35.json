{
  "id": "17f0c0a8-8bd5-4ee0-8c5f-a342c0199f35",
  "name": "Suspicious File Download From IP Via Wget.EXE",
  "description": "Detects potentially suspicious file downloads directly from IP addresses using Wget.exe",
  "source": {
    "type": "sigma",
    "id": "17f0c0a8-8bd5-4ee0-8c5f-a342c0199f35",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_wget_download_direct_ip.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_wget_download_direct_ip.yml"
  },
  "tags": [
    "attackexecution"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://www.gnu.org/software/wget/manual/wget.html"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2023-07-27",
  "modified": "2023-07-27",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\wget.exe\"}, {\"OriginalFileName\": \"wget.exe\"}]\nselection_ip: {\"CommandLine|re\": \"://[0-9]{1,3}\\\\.[0-9]{1,3}\\\\.[0-9]{1,3}\\\\.[0-9]{1,3}\"}\nselection_http: {\"CommandLine|contains\": \"http\"}\nselection_flag: [{\"CommandLine|re\": \"\\\\s-O\\\\s\"}, {\"CommandLine|contains\": \"--output-document\"}]\nselection_ext: {\"CommandLine|endswith\": [\".ps1\", \".ps1'\", \".ps1\\\"\", \".dat\", \".dat'\", \".dat\\\"\", \".msi\", \".msi'\", \".msi\\\"\", \".bat\", \".bat'\", \".bat\\\"\", \".exe\", \".exe'\", \".exe\\\"\", \".vbs\", \".vbs'\", \".vbs\\\"\", \".vbe\", \".vbe'\", \".vbe\\\"\", \".hta\", \".hta'\", \".hta\\\"\", \".dll\", \".dll'\", \".dll\\\"\", \".psm1\", \".psm1'\", \".psm1\\\"\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}