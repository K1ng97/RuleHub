  "modified": "2023-04-30",
  "mitre": {
    "tactics": [
      "TA1219"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\mstsc.exe\"}, {\"OriginalFileName\": \"mstsc.exe\"}]\nselection_cli: {\"CommandLine|endswith\": [\".rdp\", \".rdp\\\"\"]}\nfilter_optional_wsl: {\"ParentImage\": \"C:\\\\Windows\\\\System32\\\\lxss\\\\wslhost.exe\", \"CommandLine|contains\": \"C:\\\\ProgramData\\\\Microsoft\\\\WSL\\\\wslg.rdp\"}",
    "condition": "all of selection_* and not 1 of filter_optional_*",
    "fields": []
  },
  "falsepositives": [
    "Likely with legitimate usage of \".rdp\" files"
  ],
  "level": "low",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}