  "mitre": {
    "tactics": [
      "TA1574"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"ImageLoaded|endswith\": [\"\\\\libwazuhshared.dll\", \"\\\\libwinpthread-1.dll\"]}\nfilter_main_generic: {\"ImageLoaded|startswith\": [\"C:\\\\Program Files\\\\\", \"C:\\\\Program Files (x86)\\\\\"]}\nfilter_optional_mingw64: {\"ImageLoaded|contains\": [\"\\\\AppData\\\\Local\\\\\", \"\\\\ProgramData\\\\\"], \"ImageLoaded|endswith\": \"\\\\mingw64\\\\bin\\\\libwinpthread-1.dll\"}",
    "condition": "selection and not 1 of filter_main_* and not 1 of filter_optional_*",
    "fields": []
  },
  "falsepositives": [
    "Many legitimate applications leverage this DLL. (Visual Studio, JetBrains, Ruby, Anaconda, GithubDesktop, etc.)"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "image_load"
  ]
}