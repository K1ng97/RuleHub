  "id": "84b1706c-932a-44c4-ae28-892b28a25b94",
  "name": "OneNote.EXE Execution of Malicious Embedded Scripts",
  "description": "Detects the execution of malicious OneNote documents that contain embedded scripts.\nWhen a user clicks on a OneNote attachment and then on the malicious link inside the \".one\" file, it exports and executes the malicious embedded script from specific directories.\n",
  "source": {
    "type": "sigma",
    "id": "84b1706c-932a-44c4-ae28-892b28a25b94",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_office_onenote_embedded_script_execution.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_office_onenote_embedded_script_execution.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1218001"
  ],
  "author": "@kostastsale",
  "references": [
    "https://bazaar.abuse.ch/browse/tag/one/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2023-02-02",
  "modified": "2023-02-02",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"ParentImage|endswith\": \"\\\\onenote.exe\", \"Image|endswith\": [\"\\\\cmd.exe\", \"\\\\cscript.exe\", \"\\\\mshta.exe\", \"\\\\powershell.exe\", \"\\\\pwsh.exe\", \"\\\\wscript.exe\"], \"CommandLine|contains\": [\"\\\\exported\\\\\", \"\\\\onenoteofflinecache_files\\\\\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
