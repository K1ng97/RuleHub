    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_tasklist_module_enumeration.yml"
  },
  "tags": [
    "attackt1003"
  ],
  "author": "Swachchhanda Shrawan Poudel",
  "references": [
    "https://www.n00py.io/2021/05/dumping-plaintext-rdp-credentials-from-svchost-exe/",
    "https://pentestlab.blog/tag/svchost/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2024-02-12",
  "modified": "2024-03-13",
  "mitre": {
    "tactics": [
      "TA1003"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\tasklist.exe\"}, {\"OriginalFileName\": \"tasklist.exe\"}]\nselection_flags: {\"CommandLine|contains|windash\": \"-m\"}\nselection_module: {\"CommandLine|contains\": \"rdpcorets.dll\"}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
