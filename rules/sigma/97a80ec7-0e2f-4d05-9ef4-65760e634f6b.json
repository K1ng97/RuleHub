{
  "id": "97a80ec7-0e2f-4d05-9ef4-65760e634f6b",
  "name": "Security Privileges Enumeration Via Whoami.EXE",
  "description": "Detects a whoami.exe executed with the /priv command line flag instructing the tool to show all current user privileges. This is often used after a privilege escalation attempt.",
  "source": {
    "type": "sigma",
    "id": "97a80ec7-0e2f-4d05-9ef4-65760e634f6b",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_whoami_priv_discovery.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_whoami_priv_discovery.yml"
  },
  "tags": [
    "attackdiscovery",
    "attackprivilege-escalation",
    "attackt1033"
  ],
  "author": "Florian Roth (Nextron Systems)",
  "references": [
    "https://learn.microsoft.com/en-us/windows-server/administration/windows-commands/whoami"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2021-05-05",
  "modified": "2023-02-28",
  "mitre": {
    "tactics": [
      "TA1033"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\whoami.exe\"}, {\"OriginalFileName\": \"whoami.exe\"}]\nselection_cli: {\"CommandLine|contains\": [\" /priv\", \" -priv\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}