  "detection": {
    "query": "selection_name: [{\"Image|endswith\": [\"\\\\cmd.exe\", \"\\\\powershell.exe\"]}, {\"OriginalFileName\": [\"Cmd.Exe\", \"PowerShell.EXE\"]}]\nselection_args: {\"CommandLine|contains|all\": [\"echo\", \">\", \"\\\\\\\\\\\\\\\\.\\\\\\\\pipe\\\\\\\\\"]}",
    "condition": "all of selection*",
