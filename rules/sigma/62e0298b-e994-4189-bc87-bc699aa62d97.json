    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\reg.exe\"}, {\"OriginalFileName\": \"reg.exe\"}]\nselection_cli: {\"CommandLine|contains\": \" import \"}\nselection_paths: {\"CommandLine|contains\": [\"C:\\\\Users\\\\\", \"%temp%\", \"%tmp%\", \"%appdata%\", \"\\\\AppData\\\\Local\\\\Temp\\\\\", \"C:\\\\Windows\\\\Temp\\\\\", \"C:\\\\ProgramData\\\\\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate import of keys"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
