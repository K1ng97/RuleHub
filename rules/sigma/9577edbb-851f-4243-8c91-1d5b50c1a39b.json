  "detection": {
    "query": "selection: {\"TargetObject|contains\": [\"Software\\\\Microsoft\\\\Windows NT\\\\CurrentVersion\\\\Accessibility\\\\ATs\", \"Software\\\\Microsoft\\\\Windows NT\\\\CurrentVersion\\\\Accessibility\\\\Configuration\"]}\nfilter_atbroker: {\"Image\": \"C:\\\\Windows\\\\system32\\\\atbroker.exe\", \"TargetObject|contains\": \"\\\\Microsoft\\\\Windows NT\\\\CurrentVersion\\\\Accessibility\\\\Configuration\", \"Details\": \"(Empty)\"}\nfilter_uninstallers: {\"Image|startswith\": \"C:\\\\Windows\\\\Installer\\\\MSI\", \"TargetObject|contains\": \"Software\\\\Microsoft\\\\Windows NT\\\\CurrentVersion\\\\Accessibility\\\\ATs\"}",
    "condition": "selection and not 1 of filter_*",
    "fields": []
  },
  "falsepositives": [
    "Creation of non-default, legitimate at usage"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "registry_event"
  ]
}