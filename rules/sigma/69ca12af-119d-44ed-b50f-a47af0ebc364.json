  "mitre": {
    "tactics": [
      "TA1003"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": [\":\\\\Windows\\\\system32\\\\taskmgr.exe\", \":\\\\Windows\\\\SysWOW64\\\\taskmgr.exe\"], \"TargetFilename|contains|all\": [\"\\\\AppData\\\\Local\\\\Temp\\\\\", \"\\\\lsass\", \".DMP\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Rare case of troubleshooting by an administrator or support that has to be investigated regardless"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
