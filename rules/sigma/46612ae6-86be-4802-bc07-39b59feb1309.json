{
  "id": "46612ae6-86be-4802-bc07-39b59feb1309",
  "name": "Access To Windows DPAPI Master Keys By Uncommon Applications",
  "description": "Detects file access requests to the the Windows Data Protection API Master keys by an uncommon application.\nThis can be a sign of credential stealing. Example case would be usage of mimikatz \"dpapi::masterkey\" function\n",
  "source": {
    "type": "sigma",
    "id": "46612ae6-86be-4802-bc07-39b59feb1309",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/file_access_win_susp_dpapi_master_key_access.yml",
    "file_path": "tmp/repos/sigma/rules/windows/file/file_access/file_access_win_susp_dpapi_master_key_access.yml"
  },
  "tags": [
    "attackcredential-access",
    "attackt1555004"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "http://blog.harmj0y.net/redteaming/operational-guidance-for-offensive-user-dpapi-abuse/",
    "https://book.hacktricks.xyz/windows-hardening/windows-local-privilege-escalation/dpapi-extracting-passwords"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "experimental",
  "created": "2022-10-17",
  "modified": "2024-07-29",
  "mitre": {
    "tactics": [
      "TA1555"
    ],
    "techniques": [
      "T004"
    ]
  },
  "detection": {
    "query": "selection: {\"FileName|contains\": [\"\\\\Microsoft\\\\Protect\\\\S-1-5-18\\\\\", \"\\\\Microsoft\\\\Protect\\\\S-1-5-21-\"]}\nfilter_system_folders: {\"Image|startswith\": [\"C:\\\\Program Files\\\\\", \"C:\\\\Program Files (x86)\\\\\", \"C:\\\\Windows\\\\system32\\\\\", \"C:\\\\Windows\\\\SysWOW64\\\\\"]}",
    "condition": "selection and not 1 of filter_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "file_access"
  ]
}