  "detection": {
    "query": "selection_img: [{\"Image|endswith\": [\"\\\\powershell.exe\", \"\\\\pwsh.exe\"]}, {\"OriginalFileName\": [\"PowerShell.EXE\", \"pwsh.dll\"]}]\nselection_cli_enc: {\"CommandLine|contains\": \" -e\"}\nselection_cli_invoke: {\"CommandLine|contains\": [\"SQBuAHYAbwBrAGUALQ\", \"kAbgB2AG8AawBlAC0A\", \"JAG4AdgBvAGsAZQAtA\", \"SW52b2tlL\", \"ludm9rZS\", \"JbnZva2Ut\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
