  "created": "2022-11-17",
  "modified": "2022-11-17",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: [{\"Image|endswith\": \"\\\\MSBuild.exe\"}, {\"OriginalFileName\": \"MSBuild.exe\"}]\nfilter_parent: {\"ParentImage|endswith\": [\"\\\\devenv.exe\", \"\\\\cmd.exe\", \"\\\\msbuild.exe\", \"\\\\python.exe\", \"\\\\explorer.exe\", \"\\\\nuget.exe\"]}",
    "condition": "selection and not filter_parent",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}