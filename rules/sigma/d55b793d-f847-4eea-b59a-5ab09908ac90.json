  "id": "d55b793d-f847-4eea-b59a-5ab09908ac90",
  "name": "Suspicious Child Process Of Veeam Dabatase",
  "description": "Detects suspicious child processes of the Veeam service process. This could indicate potential RCE or SQL Injection.",
  "source": {
    "type": "sigma",
    "id": "d55b793d-f847-4eea-b59a-5ab09908ac90",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_mssql_veaam_susp_child_processes.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_mssql_veaam_susp_child_processes.yml"
  },
  "tags": [
    "attackinitial-access",
    "attackpersistence",
    "attackprivilege-escalation"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://labs.withsecure.com/publications/fin7-target-veeam-servers"
  ],
  "severity": "critical",
  "type": "sigma",
  "status": "test",
  "created": "2023-05-04",
  "modified": "2023-05-04",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection_parent: {\"ParentImage|endswith\": \"\\\\sqlservr.exe\", \"ParentCommandLine|contains\": \"VEEAMSQL\"}\nselection_child_1: {\"Image|endswith\": [\"\\\\cmd.exe\", \"\\\\powershell.exe\", \"\\\\pwsh.exe\", \"\\\\wsl.exe\", \"\\\\wt.exe\"], \"CommandLine|contains\": [\"-ex \", \"bypass\", \"cscript\", \"DownloadString\", \"http://\", \"https://\", \"mshta\", \"regsvr32\", \"rundll32\", \"wscript\", \"copy \"]}\nselection_child_2: {\"Image|endswith\": [\"\\\\net.exe\", \"\\\\net1.exe\", \"\\\\netstat.exe\", \"\\\\nltest.exe\", \"\\\\ping.exe\", \"\\\\tasklist.exe\", \"\\\\whoami.exe\"]}",
    "condition": "selection_parent and 1 of selection_child_*",
    "fields": []
  },
  "falsepositives": [],
  "level": "critical",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}