    "query": "selection: {\"EventType\": \"CreateKey\", \"TargetObject|contains\": [\"\\\\Active Directory Explorer\", \"\\\\Handle\", \"\\\\LiveKd\", \"\\\\Process Explorer\", \"\\\\ProcDump\", \"\\\\PsExec\", \"\\\\PsLoglist\", \"\\\\PsPasswd\", \"\\\\SDelete\", \"\\\\Sysinternals\"], \"TargetObject|endswith\": \"\\\\EulaAccepted\"}",
