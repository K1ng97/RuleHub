  "status": "test",
  "created": "2020-10-09",
  "modified": "2024-03-13",
  "mitre": {
    "tactics": [
      "TA1003"
    ],
    "techniques": []
  },
  "detection": {
    "query": "use_rpcping: {\"Image|endswith\": \"\\\\rpcping.exe\"}\nremote_server: {\"CommandLine|contains|windash\": \"-s\"}\nntlm_auth: [{\"CommandLine|contains|all|windash\": [\"-u\", \"NTLM\"]}, {\"CommandLine|contains|all|windash\": [\"-t\", \"ncacn_np\"]}]",
    "condition": "use_rpcping and remote_server and ntlm_auth",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
