  "detection": {
    "query": "selection_webdav: [{\"c-useragent|contains\": \"WebDAV\"}, {\"c-uri|contains\": \"webdav\"}]\nselection_executable: [{\"resp_mime_types|contains\": \"dosexec\"}, {\"c-uri|endswith\": \".exe\"}]",
    "condition": "selection_webdav and selection_executable",
    "fields": []
  },
