  "detection": {
    "query": "selection: {\"ImageLoaded|contains|all\": [\"\\\\VMware\\\\VMware Tools\\\\vmStatsProvider\\\\win32\", \"\\\\vmGuestLib.dll\"], \"Image|endswith\": \"\\\\Windows\\\\System32\\\\wbem\\\\WmiApSrv.exe\"}\nfilter: {\"Signed\": \"true\"}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "FP could occur if the legitimate version of vmGuestLib already exists on the system"
  ],
