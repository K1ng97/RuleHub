  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\schtasks.exe\", \"CommandLine|contains|all\": [\"/Create\", \"/RU\", \"/TR\", \"C:\\\\Users\\\\\", \"\\\\AppData\\\\Local\\\\\"], \"CommandLine|contains\": [\"NT AUT\", \" SYSTEM \"]}\nfilter: {\"ParentImage|contains|all\": [\"\\\\AppData\\\\Local\\\\Temp\\\\\", \"TeamViewer_.exe\"], \"Image|endswith\": \"\\\\schtasks.exe\", \"CommandLine|contains\": \"/TN TVInstallRestore\"}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
