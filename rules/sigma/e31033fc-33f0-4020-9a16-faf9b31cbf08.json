  "modified": "2023-02-08",
  "mitre": {
    "tactics": [
      "TA1095"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: {\"Image|endswith\": [\"\\\\nc.exe\", \"\\\\ncat.exe\", \"\\\\netcat.exe\"]}\nselection_cmdline: {\"CommandLine|contains\": [\" -lvp \", \" -lvnp\", \" -l -v -p \", \" -lv -p \", \" -l --proxy-type http \", \" -vnl --exec \", \" -vnl -e \", \" --lua-exec \", \" --sh-exec \"]}",
    "condition": "1 of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate ncat use"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
