  "detection": {
    "query": "parent_selection: {\"ParentImage|endswith\": \"\\\\cmd.exe\", \"ParentCommandLine|endswith\": \".bat\"}\nselection1: {\"Image|endswith\": \"\\\\xcopy.exe\", \"CommandLine|contains|all\": [\"powershell.exe\", \".bat.exe\"]}\nselection2: {\"Image|endswith\": \"\\\\xcopy.exe\", \"CommandLine|contains|all\": [\"pwsh.exe\", \".bat.exe\"]}\nselection3: {\"Image|endswith\": \"\\\\attrib.exe\", \"CommandLine|contains|all\": [\"+s\", \"+h\", \".bat.exe\"]}",
    "condition": "parent_selection and (1 of selection*)",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
