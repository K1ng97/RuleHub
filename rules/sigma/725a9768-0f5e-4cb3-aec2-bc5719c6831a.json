  "modified": "2022-06-29",
  "mitre": {
    "tactics": [
      "TA1217"
    ],
    "techniques": []
  },
  "detection": {
    "query": "where_exe: [{\"Image|endswith\": \"\\\\where.exe\"}, {\"OriginalFileName\": \"where.exe\"}]\nwhere_opt: {\"CommandLine|contains\": [\"places.sqlite\", \"cookies.sqlite\", \"formhistory.sqlite\", \"logins.json\", \"key4.db\", \"key3.db\", \"sessionstore.jsonlz4\", \"History\", \"Bookmarks\", \"Cookies\", \"Login Data\"]}",
    "condition": "all of where_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "low",
  "rule_format": "standard",
  "platforms": [],
