    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": [\"\\\\powershell.exe\", \"\\\\pwsh.exe\"]}, {\"OriginalFileName\": [\"PowerShell.EXE\", \"pwsh.dll\"]}]\nselection_commands: {\"CommandLine|contains\": [\"curl \", \"Invoke-WebRequest\", \"iwr \", \"wget \"]}\nselection_ip: {\"CommandLine|contains\": [\"://1\", \"://2\", \"://3\", \"://4\", \"://5\", \"://6\", \"://7\", \"://8\", \"://9\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
