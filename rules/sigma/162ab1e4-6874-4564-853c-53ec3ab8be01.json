  "detection": {
    "query": "selection1: {\"TargetFilename|endswith\": [\"\\\\TeamViewer\\\\RemotePrinting\\\\tvprint.db\", \"\\\\TeamViewer\\\\TVNetwork.log\"]}\nselection2: {\"TargetFilename|contains|all\": [\"\\\\TeamViewer\", \"_Logfile.log\"]}",
    "condition": "1 of selection*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate uses of TeamViewer in an organisation"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
