  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\msiexec.exe\"}, {\"OriginalFileName\": \"msiexec.exe\"}]\nselection_cli: {\"CommandLine|contains|windash\": [\"-i\", \"-package\", \"-a\", \"-j\"]}\nselection_quiet: {\"CommandLine|contains|windash\": \"-q\"}\nselection_remote: {\"CommandLine|contains\": [\"http\", \"\\\\\\\\\\\\\\\\\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
