  "detection": {
    "query": "selection: {\"ImageLoaded|endswith\": \"\\\\libvlc.dll\"}\nfilter_main_vlc: {\"ImageLoaded|startswith\": [\"C:\\\\Program Files (x86)\\\\VideoLAN\\\\VLC\\\\\", \"C:\\\\Program Files\\\\VideoLAN\\\\VLC\\\\\"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "False positives are expected if VLC is installed in non-default locations"
  ],
