  "created": "2022-10-30",
  "modified": "2022-10-30",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\vsls-agent.exe\", \"CommandLine|contains\": \"--agentExtensionPath\"}\nfilter: {\"CommandLine|contains\": \"Microsoft.VisualStudio.LiveShare.Agent.\"}",
    "condition": "selection and not filter",
    "fields": [
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "False positives depend on custom use of vsls-agent.exe"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
