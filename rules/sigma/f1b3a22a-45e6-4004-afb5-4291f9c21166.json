  "detection": {
    "query": "selection: {\"path|contains|all\": [\"\\\\\\\\\", \"\\\\IPC$\"], \"name|endswith\": [\"-stdin\", \"-stdout\", \"-stderr\"]}\nfilter: {\"name|startswith\": \"PSEXESVC\"}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
