      "T001",
      "T004"
    ]
  },
  "detection": {
    "query": "selection_findstr: [{\"CommandLine|contains\": \"findstr\"}, {\"Image|endswith\": \"findstr.exe\"}, {\"OriginalFileName\": \"FINDSTR.EXE\"}]\nselection_cli_download_1: {\"CommandLine|contains|windash\": \" -v \"}\nselection_cli_download_2: {\"CommandLine|contains|windash\": \" -l \"}\nselection_cli_download_3: {\"CommandLine|contains\": \"\\\\\\\\\\\\\\\\\"}",
    "condition": "selection_findstr and all of selection_cli_download_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
