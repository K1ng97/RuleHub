  "id": "970007b7-ce32-49d0-a4a4-fbef016950bd",
  "name": "Potential Configuration And Service Reconnaissance Via Reg.EXE",
  "description": "Detects the usage of \"reg.exe\" in order to query reconnaissance information from the registry. Adversaries may interact with the Windows registry to gather information about credentials, the system, configuration, and installed software.",
  "source": {
    "type": "sigma",
    "id": "970007b7-ce32-49d0-a4a4-fbef016950bd",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_reg_query_registry.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_reg_query_registry.yml"
  },
  "tags": [
    "attackdiscovery",
    "attackt1007",
    "attackt1012"
  ],
  "author": "Timur Zinniatullin, oscd.community",
  "references": [
    "https://github.com/redcanaryco/atomic-red-team/blob/f339e7da7d05f6057fdfcdd3742bfcf365fee2a9/atomics/T1012/T1012.md"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2019-10-21",
  "modified": "2023-02-05",
  "mitre": {
    "tactics": [
      "TA1007",
      "TA1012"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\reg.exe\"}, {\"OriginalFileName\": \"reg.exe\"}]\nselection_flag: {\"CommandLine|contains\": \"query\"}\nselection_key: {\"CommandLine|contains\": [\"currentVersion\\\\windows\", \"winlogon\\\\\", \"currentVersion\\\\shellServiceObjectDelayLoad\", \"currentVersion\\\\run\", \"currentVersion\\\\policies\\\\explorer\\\\run\", \"currentcontrolset\\\\services\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Discord"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}