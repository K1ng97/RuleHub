  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2017-04-15",
  "modified": "2023-02-09",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": [
      "T011"
    ]
  },
  "detection": {
    "query": "selection_parent: {\"ParentImage|endswith\": \"\\\\System32\\\\control.exe\"}\nselection_img: [{\"Image|endswith\": \"\\\\rundll32.exe\"}, {\"OriginalFileName\": \"RUNDLL32.EXE\"}]\nfilter: {\"CommandLine|contains\": \"Shell32.dll\"}",
    "condition": "all of selection_* and not filter",
    "fields": [
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}