  "detection": {
    "query": "keywords: [\"dpapi::masterkey\", \"eo.oe.kiwi\", \"event::clear\", \"event::drop\", \"gentilkiwi.com\", \"kerberos::golden\", \"kerberos::ptc\", \"kerberos::ptt\", \"kerberos::tgt\", \"Kiwi Legit Printer\", \"lsadump::\", \"mimidrv.sys\", \"\\\\mimilib.dll\", \"misc::printnightmare\", \"misc::shadowcopies\", \"misc::skeleton\", \"privilege::backup\", \"privilege::debug\", \"privilege::driver\", \"sekurlsa::\"]\nfilter: {\"EventID\": 15}",
    "condition": "keywords and not filter",
    "fields": []
  },
  "falsepositives": [
    "Naughty administrators",
    "AV Signature updates",
    "Files with Mimikatz in their filename"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
