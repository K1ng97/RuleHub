  "detection": {
    "query": "selection: {\"OriginalFileName\": \"gpg.exe\"}\nfilter_main_img: {\"Image|endswith\": [\"\\\\gpg.exe\", \"\\\\gpg2.exe\"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
