  "id": "e9c8808f-4cfb-4ba9-97d4-e5f3beaa244d",
  "name": "Windows Defender Exclusion Registry Key - Write Access Requested",
  "description": "Detects write access requests to the Windows Defender exclusions registry keys. This could be an indication of an attacker trying to request a handle or access the object to write new exclusions in order to bypass security.\n",
  "source": {
    "type": "sigma",
    "id": "e9c8808f-4cfb-4ba9-97d4-e5f3beaa244d",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/win_security_windows_defender_exclusions_write_access.yml",
    "file_path": "tmp/repos/sigma/rules/windows/builtin/security/win_security_windows_defender_exclusions_write_access.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1562001"
  ],
  "author": "@BarryShooshooga, Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://www.bleepingcomputer.com/news/security/gootkit-malware-bypasses-windows-defender-by-setting-path-exclusions/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2019-10-26",
  "modified": "2023-11-11",
  "mitre": {
    "tactics": [
      "TA1562"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"AccessList|contains\": [\"%%4417\", \"%%4418\"], \"EventID\": [4656, 4663], \"ObjectName|contains\": \"\\\\Microsoft\\\\Windows Defender\\\\Exclusions\\\\\"}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "security"
  ]
}