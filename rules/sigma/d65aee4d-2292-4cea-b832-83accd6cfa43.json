{
  "id": "d65aee4d-2292-4cea-b832-83accd6cfa43",
  "name": "Arbitrary Binary Execution Using GUP Utility",
  "description": "Detects execution of the Notepad++ updater (gup) to launch other commands or executables",
  "source": {
    "type": "sigma",
    "id": "d65aee4d-2292-4cea-b832-83accd6cfa43",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_gup_arbitrary_binary_execution.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_gup_arbitrary_binary_execution.yml"
  },
  "tags": [
    "attackexecution"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://twitter.com/nas_bench/status/1535322445439180803"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-06-10",
  "modified": "2023-03-02",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"ParentImage|endswith\": \"\\\\gup.exe\", \"Image|endswith\": \"\\\\explorer.exe\"}\nfilter: {\"Image|endswith\": \"\\\\explorer.exe\", \"CommandLine|contains\": \"\\\\Notepad++\\\\notepad++.exe\"}\nfilter_parent: {\"ParentImage|contains\": \"\\\\Notepad++\\\\updater\\\\\"}\nfilter_null: {\"CommandLine\": null}",
    "condition": "selection and not 1 of filter*",
    "fields": []
  },
  "falsepositives": [
    "Other parent binaries using GUP not currently identified"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}