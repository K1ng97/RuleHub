  "detection": {
    "query": "selection_ioc: {\"ScriptBlockText|contains\": [\".CreationTime =\", \".LastWriteTime =\", \".LastAccessTime =\", \"[IO.File]::SetCreationTime\", \"[IO.File]::SetLastAccessTime\", \"[IO.File]::SetLastWriteTime\"]}",
    "condition": "selection_ioc",
    "fields": []
  },
