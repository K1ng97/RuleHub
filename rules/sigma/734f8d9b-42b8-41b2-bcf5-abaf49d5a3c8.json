  "references": [
    "https://threathunterplaybook.com/hunts/windows/190511-RemotePwshExecution/notebook.html"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2019-09-12",
  "modified": "2022-10-09",
  "mitre": {
    "tactics": [
      "TA1021",
      "TA1059"
    ],
    "techniques": [
      "T001",
      "T006"
    ]
  },
  "detection": {
    "query": "selection: [{\"Image|endswith\": \"\\\\wsmprovhost.exe\"}, {\"ParentImage|endswith\": \"\\\\wsmprovhost.exe\"}]",
    "condition": "selection",
    "fields": [
      "ComputerName",
      "User",
      "CommandLine"
    ]
  },
  "falsepositives": [
    "Legitimate usage of remote Powershell, e.g. for monitoring purposes."
  ],
  "level": "medium",
  "rule_format": "standard",
