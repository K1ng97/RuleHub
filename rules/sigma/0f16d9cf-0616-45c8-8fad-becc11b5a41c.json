  "detection": {
    "query": "selection: [{\"Product|contains\": \"AutoHotkey\"}, {\"Description|contains\": \"AutoHotkey\"}, {\"OriginalFileName\": [\"AutoHotkey.exe\", \"AutoHotkey.rc\"]}]\nfilter: [{\"Image|endswith\": [\"\\\\AutoHotkey.exe\", \"\\\\AutoHotkey32.exe\", \"\\\\AutoHotkey32_UIA.exe\", \"\\\\AutoHotkey64.exe\", \"\\\\AutoHotkey64_UIA.exe\", \"\\\\AutoHotkeyA32.exe\", \"\\\\AutoHotkeyA32_UIA.exe\", \"\\\\AutoHotkeyU32.exe\", \"\\\\AutoHotkeyU32_UIA.exe\", \"\\\\AutoHotkeyU64.exe\", \"\\\\AutoHotkeyU64_UIA.exe\"]}, {\"Image|contains\": \"\\\\AutoHotkey\"}]",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
