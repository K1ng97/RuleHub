  "name": "Suspicious Network Connection Binary No CommandLine",
  "description": "Detects suspicious network connections made by a well-known Windows binary run with no command line parameters",
  "source": {
    "type": "sigma",
    "id": "20384606-a124-4fec-acbb-8bd373728613",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/net_connection_win_susp_binary_no_cmdline.yml",
    "file_path": "tmp/repos/sigma/rules/windows/network_connection/net_connection_win_susp_binary_no_cmdline.yml"
  },
  "tags": [
    "attackdefense-evasion"
  ],
  "author": "Florian Roth (Nextron Systems)",
  "references": [
    "https://redcanary.com/blog/raspberry-robin/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2022-07-03",
  "modified": "2022-07-03",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"Initiated\": \"true\", \"Image|endswith\": [\"\\\\regsvr32.exe\", \"\\\\rundll32.exe\", \"\\\\dllhost.exe\"], \"CommandLine|endswith\": [\"\\\\regsvr32.exe\", \"\\\\rundll32.exe\", \"\\\\dllhost.exe\"]}\nfilter_no_cmdline: {\"CommandLine\": \"\"}\nfilter_null: {\"CommandLine\": null}",
    "condition": "selection and not 1 of filter*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "network_connection"
  ]
}