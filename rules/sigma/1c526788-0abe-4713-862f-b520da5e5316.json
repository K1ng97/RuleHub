{
  "id": "1c526788-0abe-4713-862f-b520da5e5316",
  "name": "Chromium Browser Headless Execution To Mockbin Like Site",
  "description": "Detects the execution of a Chromium based browser process with the \"headless\" flag and a URL pointing to the mockbin.org service (which can be used to exfiltrate data).",
  "source": {
    "type": "sigma",
    "id": "1c526788-0abe-4713-862f-b520da5e5316",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_browsers_chromium_mockbin_abuse.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_browsers_chromium_mockbin_abuse.yml"
  },
  "tags": [
    "attackexecution"
  ],
  "author": "X__Junior (Nextron Systems)",
  "references": [
    "https://www.zscaler.com/blogs/security-research/steal-it-campaign"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2023-09-11",
  "modified": "2023-09-11",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: {\"Image|endswith\": [\"\\\\brave.exe\", \"\\\\chrome.exe\", \"\\\\msedge.exe\", \"\\\\opera.exe\", \"\\\\vivaldi.exe\"]}\nselection_headless: {\"CommandLine|contains\": \"--headless\"}\nselection_url: {\"CommandLine|contains\": [\"://run.mocky\", \"://mockbin\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}