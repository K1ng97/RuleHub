  "name": "Shell Execution via Find - Linux",
  "description": "Detects the use of the find command to execute a shell. Such behavior may be associated with privilege escalation, unauthorized command execution, or exploitation attempt.\n",
  "source": {
    "type": "sigma",
    "id": "6adfbf8f-52be-4444-9bac-81b539624146",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_lnx_find_shell_execution.yml",
    "file_path": "tmp/repos/sigma/rules/linux/process_creation/proc_creation_lnx_find_shell_execution.yml"
  },
  "tags": [
    "attackdiscovery",
    "attackt1083"
  ],
  "author": "Li Ling, Andy Parkidomo, Robert Rakowski, Blake Hartstein (Bloomberg L.P.)",
  "references": [
    "https://gtfobins.github.io/gtfobins/find/#shell",
    "https://www.elastic.co/guide/en/security/current/linux-restricted-shell-breakout-via-linux-binary-s.html"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "experimental",
  "created": "2024-09-02",
  "modified": "2024-09-02",
  "mitre": {
    "tactics": [
      "TA1083"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: {\"Image|endswith\": \"/find\", \"CommandLine|contains|all\": [\" . \", \"-exec\"]}\nselection_cli: {\"CommandLine|contains\": [\"/bin/bash\", \"/bin/dash\", \"/bin/fish\", \"/bin/sh\", \"/bin/zsh\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "linux",
    "process_creation"
  ]
}