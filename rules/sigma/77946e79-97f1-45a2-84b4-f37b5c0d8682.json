  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\regini.exe\"}, {\"OriginalFileName\": \"REGINI.EXE\"}]\nselection_re: {\"CommandLine|re\": \":[^ \\\\\\\\]\"}",
    "condition": "all of selection_*",
    "fields": [
      "ParentImage",
      "CommandLine"
    ]
  },
