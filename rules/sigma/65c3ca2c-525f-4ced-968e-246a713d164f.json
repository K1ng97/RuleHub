  "status": "test",
  "created": "2023-04-11",
  "modified": "2023-04-11",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"OriginalFileName\": \"Microsoft.NodejsTools.PressAnyKey.exe\"}\nfilter_main_legit_name: {\"Image|endswith\": \"\\\\Microsoft.NodejsTools.PressAnyKey.exe\"}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
