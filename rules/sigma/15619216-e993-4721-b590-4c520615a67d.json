{
  "id": "15619216-e993-4721-b590-4c520615a67d",
  "name": "Potential Meterpreter/CobaltStrike Activity",
  "description": "Detects the use of getsystem Meterpreter/Cobalt Strike command by detecting a specific service starting",
  "source": {
    "type": "sigma",
    "id": "15619216-e993-4721-b590-4c520615a67d",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_hktl_meterpreter_getsystem.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_hktl_meterpreter_getsystem.yml"
  },
  "tags": [
    "attackprivilege-escalation",
    "attackt1134001",
    "attackt1134002"
  ],
  "author": "Teymur Kheirkhabarov, Ecco, Florian Roth",
  "references": [
    "https://speakerdeck.com/heirhabarov/hunting-for-privilege-escalation-in-windows-environment",
    "https://blog.cobaltstrike.com/2014/04/02/what-happens-when-i-type-getsystem/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2019-10-26",
  "modified": "2023-02-05",
  "mitre": {
    "tactics": [
      "TA1134"
    ],
    "techniques": [
      "T001",
      "T002"
    ]
  },
  "detection": {
    "query": "selection_img: {\"ParentImage|endswith\": \"\\\\services.exe\"}\nselection_technique_1: {\"CommandLine|contains|all\": [\"/c\", \"echo\", \"\\\\pipe\\\\\"], \"CommandLine|contains\": [\"cmd\", \"%COMSPEC%\"]}\nselection_technique_2: {\"CommandLine|contains|all\": [\"rundll32\", \".dll,a\", \"/p:\"]}\nfilter_defender: {\"CommandLine|contains\": \"MpCmdRun\"}",
    "condition": "selection_img and 1 of selection_technique_* and not 1 of filter_*",
    "fields": [
      "ComputerName",
      "User",
      "CommandLine"
    ]
  },
  "falsepositives": [
    "Commandlines containing components like cmd accidentally",
    "Jobs and services started with cmd"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}