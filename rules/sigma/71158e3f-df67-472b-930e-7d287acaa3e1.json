{
  "id": "71158e3f-df67-472b-930e-7d287acaa3e1",
  "name": "Execution Of Non-Existing File",
  "description": "Checks whether the image specified in a process creation event is not a full, absolute path (caused by process ghosting or other unorthodox methods to start a process)",
  "source": {
    "type": "sigma",
    "id": "71158e3f-df67-472b-930e-7d287acaa3e1",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_susp_image_missing.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_susp_image_missing.yml"
  },
  "tags": [
    "attackdefense-evasion"
  ],
  "author": "Max Altgelt (Nextron Systems)",
  "references": [
    "https://pentestlaboratories.com/2021/12/08/process-ghosting/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2021-12-09",
  "modified": "2022-12-14",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "image_absolute_path: {\"Image|contains\": \"\\\\\"}\nfilter_null: {\"Image\": null}\nfilter_empty: {\"Image\": [\"-\", \"\"]}\nfilter_4688: [{\"Image\": [\"System\", \"Registry\", \"MemCompression\", \"vmmem\"]}, {\"CommandLine\": [\"Registry\", \"MemCompression\", \"vmmem\"]}]",
    "condition": "not image_absolute_path and not 1 of filter*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}