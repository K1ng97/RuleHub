  },
  "tags": [
    "attackdefense-evasion",
    "attackpersistence",
    "attackprivilege-escalation",
    "attackt1574001"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://labs.withsecure.com/publications/fin7-target-veeam-servers"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2023-05-05",
  "modified": "2023-05-05",
  "mitre": {
    "tactics": [
      "TA1574"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\gup.exe\", \"ImageLoaded|endswith\": \"\\\\libcurl.dll\"}\nfilter_main_notepad_plusplus: {\"Image|endswith\": \"\\\\Notepad++\\\\updater\\\\GUP.exe\"}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "image_load"
  ]
