  "detection": {
    "query": "selection: {\"Image|endswith\": [\"\\\\powershell.exe\", \"\\\\pwsh.exe\", \"\\\\cmd.exe\"], \"ParentCommandLine|contains|all\": [\"MsiExec.exe\", \"-Embedding \"]}\nfilter_splunk_ufw: {\"Image|endswith\": \":\\\\Windows\\\\System32\\\\cmd.exe\", \"CommandLine|contains\": \"C:\\\\Program Files\\\\SplunkUniversalForwarder\\\\bin\\\\\"}\nfilter_vs: [{\"CommandLine|contains\": \"\\\\DismFoDInstall.cmd\"}, {\"ParentCommandLine|contains|all\": [\"\\\\MsiExec.exe -Embedding \", \"Global\\\\MSI0000\"]}]",
    "condition": "selection and not 1 of filter*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
