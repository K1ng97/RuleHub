  },
  "detection": {
    "query": "selection: {\"CommandLine|contains\": \".SettingContent-ms\"}\nfilter: {\"CommandLine|contains\": \"immersivecontrolpanel\"}",
    "condition": "selection and not filter",
    "fields": [
      "ParentProcess",
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "Unknown"
