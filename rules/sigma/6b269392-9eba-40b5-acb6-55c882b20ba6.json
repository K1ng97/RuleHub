  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\w3wp.exe\", \"CommandLine|contains\": \"MSExchange\"}\nselection_types: {\"TargetFilename|endswith\": [\".aspx\", \".asp\", \".ashx\", \".ps1\", \".bat\", \".exe\", \".dll\", \".vbs\"]}",
    "condition": "all of selection*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
