    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_iis_appcmd_susp_rewrite_rule.yml"
  },
  "tags": [
    "attackdefense-evasion"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://twitter.com/malmoeb/status/1616702107242971144",
    "https://learn.microsoft.com/en-us/answers/questions/739120/how-to-add-re-write-global-rule-with-action-type-r"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2023-01-22",
  "modified": "2023-01-22",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\appcmd.exe\"}, {\"OriginalFileName\": \"appcmd.exe\"}]\nselection_cli: {\"CommandLine|contains|all\": [\"set\", \"config\", \"section:system.webServer/rewrite/globalRules\", \"commit:\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate usage of appcmd to add new URL rewrite rules"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}