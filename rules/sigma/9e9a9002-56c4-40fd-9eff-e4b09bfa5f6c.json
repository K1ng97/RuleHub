  },
  "tags": [
    "attackdefense-evasion",
    "attackt1070"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://github.com/hackerhouse-opensource/iscsicpl_bypassUAC (Idea)"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-07-17",
  "modified": "2023-09-18",
  "mitre": {
    "tactics": [
      "TA1070"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"Image|startswith\": \"C:\\\\Windows\\\\\", \"ImageLoaded|startswith\": [\"C:\\\\Users\\\\Public\\\\\", \"C:\\\\PerfLogs\\\\\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
