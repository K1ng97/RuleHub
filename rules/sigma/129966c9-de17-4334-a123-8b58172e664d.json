{
  "id": "129966c9-de17-4334-a123-8b58172e664d",
  "name": "Potential Windows Defender AV Bypass Via Dump64.EXE Rename",
  "description": "Detects when a user is potentially trying to bypass the Windows Defender AV by renaming a tool to dump64.exe and placing it in the Visual Studio folder.\nCurrently the rule is covering only usage of procdump but other utilities can be added in order to increase coverage.\n",
  "source": {
    "type": "sigma",
    "id": "129966c9-de17-4334-a123-8b58172e664d",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_dump64_defender_av_bypass_rename.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_dump64_defender_av_bypass_rename.yml"
  },
  "tags": [
    "attackcredential-access",
    "attackt1003001"
  ],
  "author": "Austin Songer @austinsonger, Florian Roth (Nextron Systems), Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://twitter.com/mrd0x/status/1460597833917251595"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2021-11-26",
  "modified": "2024-06-21",
  "mitre": {
    "tactics": [
      "TA1003"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection_dump: {\"Image|startswith\": \":\\\\Program Files\", \"Image|contains\": \"\\\\Microsoft Visual Studio\\\\\", \"Image|endswith\": \"\\\\dump64.exe\"}\nselection_tools_procdump: [{\"OriginalFileName\": \"procdump\"}, {\"CommandLine|contains\": [\" -ma \", \" -mp \"]}]",
    "condition": "selection_dump and 1 of selection_tools_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}