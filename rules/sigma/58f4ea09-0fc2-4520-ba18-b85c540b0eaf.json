  "detection": {
    "query": "selection: {\"ParentImage|endswith\": \"\\\\Serv-U.exe\", \"Image|endswith\": [\"\\\\cmd.exe\", \"\\\\powershell.exe\", \"\\\\pwsh.exe\", \"\\\\wscript.exe\", \"\\\\cscript.exe\", \"\\\\sh.exe\", \"\\\\bash.exe\", \"\\\\schtasks.exe\", \"\\\\regsvr32.exe\", \"\\\\wmic.exe\", \"\\\\mshta.exe\", \"\\\\rundll32.exe\", \"\\\\msiexec.exe\", \"\\\\forfiles.exe\", \"\\\\scriptrunner.exe\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Legitimate uses in which users or programs use the SSH service of Serv-U for remote command execution"
  ],
  "level": "high",
  "rule_format": "standard",
