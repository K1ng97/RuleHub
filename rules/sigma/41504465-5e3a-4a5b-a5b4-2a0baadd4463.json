  "detection": {
    "query": "selection: {\"PipeName\": \"\\\\PSEXESVC\", \"Image|contains\": [\":\\\\Users\\\\Public\\\\\", \":\\\\Windows\\\\Temp\\\\\", \"\\\\AppData\\\\Local\\\\Temp\\\\\", \"\\\\Desktop\\\\\", \"\\\\Downloads\\\\\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Rare legitimate use of psexec from the locations mentioned above. This will require initial tuning based on your environment."
