{
  "id": "d2eb17db-1d39-41dc-b57f-301f6512fa75",
  "name": "Potentially Suspicious Command Targeting Teams Sensitive Files",
  "description": "Detects a commandline containing references to the Microsoft Teams database or cookies files from a process other than Teams.\nThe database might contain authentication tokens and other sensitive information about the logged in accounts.\n",
  "source": {
    "type": "sigma",
    "id": "d2eb17db-1d39-41dc-b57f-301f6512fa75",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_teams_suspicious_command_line_cred_access.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_teams_suspicious_command_line_cred_access.yml"
  },
  "tags": [
    "attackcredential-access",
    "attackt1528"
  ],
  "author": "@SerkinValery",
  "references": [
    "https://www.bleepingcomputer.com/news/security/microsoft-teams-stores-auth-tokens-as-cleartext-in-windows-linux-macs/",
    "https://www.vectra.ai/blogpost/undermining-microsoft-teams-security-by-mining-tokens"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-09-16",
  "modified": "2023-12-18",
  "mitre": {
    "tactics": [
      "TA1528"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"CommandLine|contains\": [\"\\\\Microsoft\\\\Teams\\\\Cookies\", \"\\\\Microsoft\\\\Teams\\\\Local Storage\\\\leveldb\"]}\nfilter_main_legit_locations: {\"Image|endswith\": \"\\\\Microsoft\\\\Teams\\\\current\\\\Teams.exe\"}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}