  "detection": {
    "query": "selection_gen: {\"TargetFilename|endswith\": [\".exe\", \".iso\", \".rar\", \".zip\"], \"TargetFilename|contains\": [\".doc.\", \".docx.\", \".jpg.\", \".pdf.\", \".ppt.\", \".pptx.\", \".xls.\", \".xlsx.\"]}\nselection_exe: {\"TargetFilename|endswith\": [\".rar.exe\", \".zip.exe\"]}",
    "condition": "1 of selection_*",
    "fields": []
  },
