  "detection": {
    "query": "selection: {\"EventID\": 4624, \"LogonType\": 3, \"AuthenticationPackageName\": \"Kerberos\", \"IpAddress\": \"127.0.0.1\", \"TargetUserSid|startswith\": \"S-1-5-21-\", \"TargetUserSid|endswith\": \"-500\"}\nfilter_main_ip_null: {\"IpPort\": \"0\"}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
