    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_netsh_fw_allow_rdp.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1562004"
  ],
  "author": "Sander Wiebing",
  "references": [
    "https://labs.sentinelone.com/sarwent-malware-updates-command-detonation/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2020-05-23",
  "modified": "2023-12-11",
  "mitre": {
    "tactics": [
      "TA1562"
    ],
    "techniques": [
      "T004"
    ]
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\netsh.exe\"}, {\"OriginalFileName\": \"netsh.exe\"}]\nselection_cli: {\"CommandLine|contains|all\": [\"firewall \", \"add \", \"tcp \", \"3389\"], \"CommandLine|contains\": [\"portopening\", \"allow\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate administration activity"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
