  },
  "tags": [
    "attackexfiltration",
    "attackt1048"
  ],
  "author": "Florian Roth (Nextron Systems)",
  "references": [
    "https://www.microsoft.com/security/blog/2022/01/15/destructive-malware-targeting-ukrainian-organizations/",
    "http://blog.talosintelligence.com/2022/09/lazarus-three-rats.html"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2022-01-16",
  "modified": "2023-12-28",
  "mitre": {
    "tactics": [
      "TA1048"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_redirect: {\"CommandLine|contains\": \">\"}\nselection_share: {\"CommandLine|contains\": [\"\\\\\\\\\\\\\\\\127.0.0.1\\\\\\\\admin$\\\\\\\\\", \"\\\\\\\\\\\\\\\\localhost\\\\\\\\admin$\\\\\\\\\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
