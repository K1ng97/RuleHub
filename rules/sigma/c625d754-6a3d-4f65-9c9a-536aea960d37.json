    "tactics": [
      "TA1069"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection_img: [{\"Product|endswith\": \"AccessChk\"}, {\"Description|contains\": \"Reports effective permissions\"}, {\"Image|endswith\": [\"\\\\accesschk.exe\", \"\\\\accesschk64.exe\"]}, {\"OriginalFileName\": \"accesschk.exe\"}]\nselection_cli: {\"CommandLine|contains\": [\"uwcqv \", \"kwsu \", \"qwsu \", \"uwdqs \"]}",
    "condition": "all of selection*",
    "fields": []
  },
  "falsepositives": [
    "System administrator Usage"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
