  "name": "Suspicious Unsigned Thor Scanner Execution",
  "description": "Detects loading and execution of an unsigned thor scanner binary.",
  "source": {
    "type": "sigma",
    "id": "ea5c131b-380d-49f9-aeb3-920694da4d4b",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/image_load_thor_unsigned_execution.yml",
    "file_path": "tmp/repos/sigma/rules/windows/image_load/image_load_thor_unsigned_execution.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1574001"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "Internal Research"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "stable",
  "created": "2023-10-29",
  "modified": "2023-10-29",
  "mitre": {
    "tactics": [
      "TA1574"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": [\"\\\\thor.exe\", \"\\\\thor64.exe\"], \"ImageLoaded|endswith\": [\"\\\\thor.exe\", \"\\\\thor64.exe\"]}\nfilter_main: {\"Signed\": \"true\", \"SignatureStatus\": \"valid\", \"Signature\": \"Nextron Systems GmbH\"}",
    "condition": "selection and not filter_main",
    "fields": []
  },
  "falsepositives": [
    "Other legitimate binaries named \"thor.exe\" that aren't published by Nextron Systems"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "image_load"
  ]
}