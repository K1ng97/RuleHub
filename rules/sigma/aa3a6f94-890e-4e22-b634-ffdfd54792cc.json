  "mitre": {
    "tactics": [
      "TA1204"
    ],
    "techniques": [
      "T002"
    ]
  },
  "detection": {
    "query": "selection: {\"ParentImage|endswith\": [\"\\\\WINWORD.EXE\", \"\\\\EXCEL.EXE\", \"\\\\POWERPNT.exe\", \"\\\\MSPUB.exe\", \"\\\\VISIO.exe\", \"\\\\MSACCESS.exe\", \"\\\\EQNEDT32.exe\"], \"Image|startswith\": \"C:\\\\users\\\\\", \"Image|endswith\": \".exe\"}\nfilter: {\"Image|endswith\": \"\\\\Teams.exe\"}",
    "condition": "selection and not filter",
    "fields": [
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
