{
  "id": "5ef9853e-4d0e-4a70-846f-a9ca37d876da",
  "name": "Potential Credential Dumping Activity Via LSASS",
  "description": "Detects process access requests to the LSASS process with specific call trace calls and access masks.\nThis behaviour is expressed by many credential dumping tools such as Mimikatz, NanoDump, Invoke-Mimikatz, Procdump and even the Taskmgr dumping feature.\n",
  "source": {
    "type": "sigma",
    "id": "5ef9853e-4d0e-4a70-846f-a9ca37d876da",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_access_win_lsass_memdump.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_access/proc_access_win_lsass_memdump.yml"
  },
  "tags": [
    "attackcredential-access",
    "attacks0002",
    "attackt1003001"
  ],
  "author": "Samir Bousseaden, Michael Haag",
  "references": [
    "https://web.archive.org/web/20230329170326/https://blog.menasec.net/2019/02/threat-hunting-21-procdump-or-taskmgr.html",
    "https://web.archive.org/web/20230208123920/https://cyberwardog.blogspot.com/2017/03/chronicles-of-threat-hunter-hunting-for_22.html",
    "https://github.com/redcanaryco/atomic-red-team/blob/f339e7da7d05f6057fdfcdd3742bfcf365fee2a9/atomics/T1003.001/T1003.001.md",
    "https://research.splunk.com/endpoint/windows_possible_credential_dumping/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2019-04-03",
  "modified": "2024-03-02",
  "mitre": {
    "tactics": [
      "TA1003"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"TargetImage|endswith\": \"\\\\lsass.exe\", \"GrantedAccess|contains\": [\"0x1038\", \"0x1438\", \"0x143a\", \"0x1fffff\"], \"CallTrace|contains\": [\"dbgcore.dll\", \"dbghelp.dll\", \"kernel32.dll\", \"kernelbase.dll\", \"ntdll.dll\"]}\nfilter_main_system_user: {\"SourceUser|contains\": [\"AUTHORI\", \"AUTORI\"]}\nfilter_optional_thor: {\"CallTrace|contains|all\": [\":\\\\Windows\\\\Temp\\\\asgard2-agent\\\\\", \"\\\\thor\\\\thor64.exe+\", \"|UNKNOWN(\"], \"GrantedAccess\": \"0x103800\"}\nfilter_optional_sysmon: {\"SourceImage|endswith\": \":\\\\Windows\\\\Sysmon64.exe\"}",
    "condition": "selection and not 1 of filter_main_* and not 1 of filter_optional_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_access"
  ]
}