  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\rdrleakdiag.exe\"}, {\"OriginalFileName\": \"RdrLeakDiag.exe\"}]\nselection_cli_dump: {\"CommandLine|contains|windash\": [\"/memdmp\", \"fullmemdmp\"]}\nselection_cli_output_process: {\"CommandLine|contains|windash\": [\" /o \", \" /p \"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
