  "detection": {
    "query": "selection: {\"CommandLine|endswith\": \"svchost.exe\", \"Image|endswith\": \"\\\\svchost.exe\"}\nfilter: [{\"ParentImage|endswith\": [\"\\\\rpcnet.exe\", \"\\\\rpcnetp.exe\"]}, {\"CommandLine\": null}]",
    "condition": "selection and not filter",
    "fields": [
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "Rpcnet.exe / rpcnetp.exe which is a lojack style software. https://www.blackhat.com/docs/us-14/materials/us-14-Kamlyuk-Kamluk-Computrace-Backdoor-Revisited.pdf"
  ],
