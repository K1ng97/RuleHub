  "detection": {
    "query": "selection_plain: {\"Image|endswith\": \"\\\\dtrace.exe\", \"CommandLine|contains\": \"lkd(0)\"}\nselection_obfuscated: {\"CommandLine|contains|all\": [\"syscall:::return\", \"lkd(\"]}",
    "condition": "1 of selection*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
