  "detection": {
    "query": "selection: {\"TargetObject|contains\": \"\\\\Microsoft\\\\Windows\\\\CurrentVersion\\\\WINEVT\\\\Channels\\\\\", \"TargetObject|endswith\": \"\\\\ChannelAccess\", \"Details|contains\": [\"(A;;0x1;;;LA)\", \"(A;;0x1;;;SY)\", \"(A;;0x5;;;BA)\"]}\nfilter_main_trustedinstaller: {\"Image\": \"C:\\\\Windows\\\\servicing\\\\TrustedInstaller.exe\"}\nfilter_main_tiworker: {\"Image|startswith\": \"C:\\\\Windows\\\\WinSxS\\\\\", \"Image|endswith\": \"\\\\TiWorker.exe\"}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
