  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2023-07-12",
  "modified": "2023-12-15",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"Initiated\": \"true\", \"Image|endswith\": \"\\\\wordpad.exe\"}\nfilter_main_ports: {\"DestinationPort\": [80, 139, 443, 445, 465, 587, 993, 995]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Other ports can be used, apply additional filters accordingly"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
