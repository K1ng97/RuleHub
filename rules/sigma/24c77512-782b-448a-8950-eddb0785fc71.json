  "detection": {
    "query": "selection_sql: [{\"Product\": \"SQLite\"}, {\"Image|endswith\": [\"\\\\sqlite.exe\", \"\\\\sqlite3.exe\"]}]\nselection_chromium: {\"CommandLine|contains\": [\"\\\\User Data\\\\\", \"\\\\Opera Software\\\\\", \"\\\\ChromiumViewer\\\\\"]}\nselection_data: {\"CommandLine|contains\": [\"Login Data\", \"Cookies\", \"Web Data\", \"History\", \"Bookmarks\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
