  "id": "a383dec4-deec-4e6e-913b-ed9249670848",
  "name": "Potential Signing Bypass Via Windows Developer Features",
  "description": "Detects when a user enable developer features such as \"Developer Mode\" or \"Application Sideloading\". Which allows the user to install untrusted packages.",
  "source": {
    "type": "sigma",
    "id": "a383dec4-deec-4e6e-913b-ed9249670848",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_systemsettingsadminflows_turn_on_dev_features.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_systemsettingsadminflows_turn_on_dev_features.yml"
  },
  "tags": [
    "attackdefense-evasion"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "Internal Research",
    "https://www.sentinelone.com/labs/inside-malicious-windows-apps-for-malware-deployment/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2023-01-11",
  "modified": "2023-01-11",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\SystemSettingsAdminFlows.exe\"}, {\"OriginalFileName\": \"SystemSettingsAdminFlows.EXE\"}]\nselection_flag: {\"CommandLine|contains\": \"TurnOnDeveloperFeatures\"}\nselection_options: {\"CommandLine|contains\": [\"DeveloperUnlock\", \"EnableSideloading\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}