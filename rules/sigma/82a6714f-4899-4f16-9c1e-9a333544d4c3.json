  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\certutil.exe\"}, {\"OriginalFileName\": \"CertUtil.exe\"}]\nselection_cli: {\"CommandLine|contains|windash\": \"-encode\"}\nselection_extension: {\"CommandLine|contains\": [\"\\\\AppData\\\\Roaming\\\\\", \"\\\\Desktop\\\\\", \"\\\\Local\\\\Temp\\\\\", \"\\\\PerfLogs\\\\\", \"\\\\Users\\\\Public\\\\\", \"\\\\Windows\\\\Temp\\\\\", \"$Recycle.Bin\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
