  "detection": {
    "query": "selection: {\"TargetFilename|startswith\": \"C:\\\\Windows\\\\WinSxS\\\\\", \"TargetFilename|endswith\": \".exe\"}\nfilter_main_system_location: {\"Image|startswith\": [\"C:\\\\Windows\\\\Systems32\\\\\", \"C:\\\\Windows\\\\SysWOW64\\\\\", \"C:\\\\Windows\\\\WinSxS\\\\\"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
