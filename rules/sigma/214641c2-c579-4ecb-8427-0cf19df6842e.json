  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\desktopimgdownldr.exe\", \"ParentImage|endswith\": \"\\\\desktopimgdownldr.exe\", \"CommandLine|contains\": \"/lockscreenurl:http\"}",
    "condition": "selection",
    "fields": []
  },
