{
  "id": "05c36dd6-79d6-4a9a-97da-3db20298ab2d",
  "name": "XSL Script Execution Via WMIC.EXE",
  "description": "Detects the execution of WMIC with the \"format\" flag to potentially load XSL files.\nAdversaries abuse this functionality to execute arbitrary files while potentially bypassing application whitelisting defenses.\nExtensible Stylesheet Language (XSL) files are commonly used to describe the processing and rendering of data within XML files.\n",
  "source": {
    "type": "sigma",
    "id": "05c36dd6-79d6-4a9a-97da-3db20298ab2d",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_wmic_xsl_script_processing.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_wmic_xsl_script_processing.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1220"
  ],
  "author": "Timur Zinniatullin, oscd.community, Swachchhanda Shrawan Poudel",
  "references": [
    "https://github.com/redcanaryco/atomic-red-team/blob/f339e7da7d05f6057fdfcdd3742bfcf365fee2a9/atomics/T1220/T1220.md"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2019-10-21",
  "modified": "2024-03-05",
  "mitre": {
    "tactics": [
      "TA1220"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\wmic.exe\", \"CommandLine|contains|windash\": \"-format\"}\nfilter_main_known_format: {\"CommandLine|contains\": [\"Format:List\", \"Format:htable\", \"Format:hform\", \"Format:table\", \"Format:mof\", \"Format:value\", \"Format:rawxml\", \"Format:xml\", \"Format:csv\"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "WMIC.exe FP depend on scripts and administrative methods used in the monitored environment.",
    "Static format arguments - https://petri.com/command-line-wmi-part-3"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}