  "detection": {
    "query": "selection_path: {\"TargetFilename|startswith\": \"C:\\\\Windows\\\\Temp\\\\\"}\nselection_names_str: {\"TargetFilename|endswith\": [\"\\\\temp.ps1\", \"\\\\msol.ps1\"]}\nselection_names_re: [{\"TargetFilename|re\": \"\\\\\\\\[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}\\\\.txt$\"}, {\"TargetFilename|re\": \"\\\\\\\\[a-zA-Z]{8}\\\\.tmp$\"}]",
    "condition": "selection_path and 1 of selection_names_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "file_event"
  ]
