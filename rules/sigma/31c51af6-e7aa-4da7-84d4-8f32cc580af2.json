  "detection": {
    "query": "selection_eid: {\"Provider_Name\": \"Service Control Manager\", \"EventID\": 7045}\nselection_service_1: {\"ImagePath|re\": \"^[a-zA-Z]:\\\\\\\\windows\\\\\\\\temp\\\\\\\\[a-zA-Z0-9]{10}\\\\.exe\"}\nselection_service_2: {\"ServiceName\": [\"Sliver\", \"Sliver implant\"]}",
    "condition": "selection_eid and 1 of selection_service_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
