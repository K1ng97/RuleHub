    "query": "selection: {\"ParentImage|endswith\": \"\\\\vsjitdebugger.exe\"}\nreduction1: {\"Image|endswith\": \"\\\\vsimmersiveactivatehelper*.exe\"}\nreduction2: {\"Image|endswith\": \"\\\\devenv.exe\"}",
    "condition": "selection and not (reduction1 or reduction2)",
    "fields": []
  },
  "falsepositives": [
    "The process spawned by vsjitdebugger.exe is uncommon."
