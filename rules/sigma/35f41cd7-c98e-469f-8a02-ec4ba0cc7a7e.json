  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"ScriptBlockText|contains|all\": [\"Write-EventLog\", \"-RawData \"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Legitimate applications writing events via this cmdlet. Investigate alerts to determine if the action is benign"
