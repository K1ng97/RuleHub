  "detection": {
    "query": "selection: {\"CommandLine|contains\": \" run run-cmd \"}\nfilter_main_legit_parents: {\"ParentImage|endswith\": [\"\\\\cleanapi.exe\", \"\\\\kavremover.exe\"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
