{
  "id": "2d3cdeec-c0db-45b4-aa86-082f7eb75701",
  "name": "Microsoft IIS Service Account Password Dumped",
  "description": "Detects the Internet Information Services (IIS) command-line tool, AppCmd, being used to list passwords",
  "source": {
    "type": "sigma",
    "id": "2d3cdeec-c0db-45b4-aa86-082f7eb75701",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_iis_appcmd_service_account_password_dumped.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_iis_appcmd_service_account_password_dumped.yml"
  },
  "tags": [
    "attackcredential-access",
    "attackt1003"
  ],
  "author": "Tim Rauch, Janantha Marasinghe, Elastic (original idea)",
  "references": [
    "https://www.elastic.co/guide/en/security/current/microsoft-iis-service-account-password-dumped.html",
    "https://twitter.com/0gtweet/status/1588815661085917186?cxt=HHwWhIDUyaDbzYwsAAAA",
    "https://www.netspi.com/blog/technical/network-penetration-testing/decrypting-iis-passwords-to-break-out-of-the-dmz-part-2/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2022-11-08",
  "modified": "2023-01-22",
  "mitre": {
    "tactics": [
      "TA1003"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_base_name: [{\"Image|endswith\": \"\\\\appcmd.exe\"}, {\"OriginalFileName\": \"appcmd.exe\"}]\nselection_base_list: {\"CommandLine|contains\": \"list \"}\nselection_standalone: {\"CommandLine|contains\": [\" /config\", \" /xml\", \" -config\", \" -xml\"]}\nselection_cmd_flags: {\"CommandLine|contains\": [\" /@t\", \" /text\", \" /show\", \" -@t\", \" -text\", \" -show\"]}\nselection_cmd_grep: {\"CommandLine|contains\": [\":\\\\*\", \"password\"]}",
    "condition": "all of selection_base_* and (selection_standalone or all of selection_cmd_*)",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}