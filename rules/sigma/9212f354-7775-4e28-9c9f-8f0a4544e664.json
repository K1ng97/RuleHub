  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\ADExplorer.exe\"}, {\"OriginalFileName\": \"AdExp\"}]\nselection_cli: {\"CommandLine|contains\": \"snapshot\"}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
