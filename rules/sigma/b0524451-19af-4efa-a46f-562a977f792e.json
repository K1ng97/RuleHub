  "detection": {
    "query": "selection1a: {\"CommandLine|contains|all\": [\"rundll32\", \"apphelp.dll\"]}\nselection1b: {\"CommandLine|contains\": [\"ShimFlushCache\", \"#250\"]}\nselection2a: {\"CommandLine|contains|all\": [\"rundll32\", \"kernel32.dll\"]}\nselection2b: {\"CommandLine|contains\": [\"BaseFlushAppcompatCache\", \"#46\"]}",
    "condition": "( selection1a and selection1b ) or ( selection2a and selection2b )",
    "fields": [
      "Image",
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "Unknown"
  ],
