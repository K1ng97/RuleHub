{
  "id": "9c8c7000-3065-44a8-a555-79bcba5d9955",
  "name": "Execute MSDT Via Answer File",
  "description": "Detects execution of \"msdt.exe\" using an answer file which is simulating the legitimate way of calling msdt via \"pcwrun.exe\" (For example from the compatibility tab)",
  "source": {
    "type": "sigma",
    "id": "9c8c7000-3065-44a8-a555-79bcba5d9955",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_lolbin_msdt_answer_file.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_lolbin_msdt_answer_file.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackexecution",
    "attackt1218"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://lolbas-project.github.io/lolbas/Binaries/Msdt/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2022-06-13",
  "modified": "2022-06-13",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_cli: {\"Image|endswith\": \"\\\\msdt.exe\", \"CommandLine|contains\": \"\\\\WINDOWS\\\\diagnostics\\\\index\\\\PCWDiagnostic.xml\"}\nselection_answer: {\"CommandLine|contains\": [\" -af \", \" /af \"]}\nfilter: {\"ParentImage|endswith\": \"\\\\pcwrun.exe\"}",
    "condition": "all of selection* and not filter",
    "fields": []
  },
  "falsepositives": [
    "Possible undocumented parents of \"msdt\" other than \"pcwrun\""
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}