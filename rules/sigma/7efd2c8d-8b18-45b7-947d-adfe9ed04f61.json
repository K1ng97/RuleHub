{
  "id": "7efd2c8d-8b18-45b7-947d-adfe9ed04f61",
  "name": "AgentExecutor PowerShell Execution",
  "description": "Detects execution of the AgentExecutor.exe binary. Which can be abused as a LOLBIN to execute powershell scripts with the ExecutionPolicy \"Bypass\" or any binary named \"powershell.exe\" located in the path provided by 6th positional argument",
  "source": {
    "type": "sigma",
    "id": "7efd2c8d-8b18-45b7-947d-adfe9ed04f61",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_agentexecutor_potential_abuse.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_agentexecutor_potential_abuse.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1218"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems), memory-shards",
  "references": [
    "https://twitter.com/lefterispan/status/1286259016436514816",
    "https://lolbas-project.github.io/lolbas/OtherMSBinaries/Agentexecutor/",
    "https://learn.microsoft.com/en-us/mem/intune/apps/intune-management-extension",
    "https://twitter.com/jseerden/status/1247985304667066373/photo/1"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-12-24",
  "modified": "2024-08-07",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image\": \"\\\\AgentExecutor.exe\"}, {\"OriginalFileName\": \"AgentExecutor.exe\"}]\nselection_cli: {\"CommandLine|contains\": [\" -powershell\", \" -remediationScript\"]}\nfilter_main_intune: {\"ParentImage|endswith\": \"\\\\Microsoft.Management.Services.IntuneWindowsAgent.exe\"}",
    "condition": "all of selection_* and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate use via Intune management. You exclude script paths and names to reduce FP rate"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}