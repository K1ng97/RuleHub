      "TA1546"
    ],
    "techniques": [
      "T007"
    ]
  },
  "detection": {
    "query": "selection_img: [{\"OriginalFileName\": \"netsh.exe\"}, {\"Image|endswith\": \"\\\\netsh.exe\"}]\nselection_cli: {\"CommandLine|contains|all\": [\"add\", \"helper\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
