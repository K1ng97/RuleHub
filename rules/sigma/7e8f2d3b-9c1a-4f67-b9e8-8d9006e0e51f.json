  "mitre": {
    "tactics": [
      "TA1548"
    ],
    "techniques": [
      "T002"
    ]
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\dism.exe\"}, {\"OriginalFileName\": \"DISM.EXE\"}]\nselection_cli: {\"CommandLine|contains|all\": [\"WindowsPowerShellWebAccess\", \"/online\", \"/enable-feature\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate PowerShell Web Access installations by administrators"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}