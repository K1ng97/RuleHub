    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_dism_enable_powershell_web_access_feature.yml"
  },
  "tags": [
    "attackpersistence",
    "attackt1548002"
  ],
  "author": "Michael Haag",
  "references": [
    "https://docs.microsoft.com/en-us/powershell/module/dism/enable-windowsoptionalfeature",
    "https://www.cisa.gov/news-events/cybersecurity-advisories/aa24-241a",
    "https://gist.github.com/MHaggis/7e67b659af9148fa593cf2402edebb41"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2024-09-03",
