  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\netsh.exe\"}, {\"OriginalFileName\": \"netsh.exe\"}]\nselection_cli: {\"CommandLine|contains|all\": [\"firewall\", \"delete \"]}\nfilter_optional_dropbox: {\"ParentImage|endswith\": \"\\\\Dropbox.exe\", \"CommandLine|contains\": \"name=Dropbox\"}",
    "condition": "all of selection_* and not 1 of filter_optional_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate administration activity",
    "Software installations and removal"
  ],
