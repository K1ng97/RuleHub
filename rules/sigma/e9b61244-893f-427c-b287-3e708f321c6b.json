  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\cmd.exe\"}, {\"OriginalFileName\": \"Cmd.Exe\"}]\nselection_cli: {\"CommandLine|contains|all\": [\"mklink\", \"\\\\osk.exe\", \"\\\\cmd.exe\"]}",
