  "detection": {
    "query": "selection: {\"ScriptBlockText|contains\": [\"-Properties*TrustedForDelegation\", \"-Properties*TrustedToAuthForDelegation\", \"-Properties*msDS-AllowedToDelegateTo\", \"-Properties*PrincipalsAllowedToDelegateToAccount\", \"-LDAPFilter*(userAccountControl:1.2.840.113556.1.4.803:=524288)\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Legitimate use of the library for administrative activity"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "ps_script"
  ]
