    "file_path": "tmp/repos/sigma/rules/windows/process_access/proc_access_win_lsass_werfault.yml"
  },
  "tags": [
    "attackcredential-access",
    "attacks0002",
    "attackt1003001"
  ],
  "author": "Florian Roth (Nextron Systems)",
  "references": [
    "https://github.com/helpsystems/nanodump/commit/578116faea3d278d53d70ea932e2bbfe42569507"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2012-06-27",
  "modified": "2023-11-29",
  "mitre": {
    "tactics": [
      "TA1003"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"SourceImage|endswith\": \"\\\\WerFault.exe\", \"TargetImage|endswith\": \"\\\\lsass.exe\", \"GrantedAccess\": \"0x1FFFFF\"}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Actual failures in lsass.exe that trigger a crash dump (unlikely)",
    "Unknown cases in which WerFault accesses lsass.exe"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_access"
  ]
}