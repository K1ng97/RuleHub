  "modified": "2024-11-23",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: [{\"Product|contains\": \"NetSupport Remote Control\"}, {\"OriginalFileName|contains\": \"client32.exe\"}, {\"Hashes|contains\": \"IMPHASH=A9D50692E95B79723F3E76FCF70D023E\"}]\nfilter: {\"Image|endswith\": \"\\\\client32.exe\"}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
