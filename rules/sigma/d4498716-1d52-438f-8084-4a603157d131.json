  "detection": {
    "query": "selection_img: [{\"Image|endswith\": [\"\\\\net.exe\", \"\\\\net1.exe\"]}, {\"OriginalFileName\": [\"net.exe\", \"net1.exe\"]}]\nselection_cli: {\"CommandLine|contains|all\": [\" use \", \":*\\\\\\\\\", \"/USER:* *\"]}\nfilter_main_empty: {\"CommandLine|endswith\": \" \"}",
    "condition": "all of selection_* and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
