  "modified": "2023-02-14",
  "mitre": {
    "tactics": [
      "TA1047"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\WMIC.exe\"}, {\"OriginalFileName\": \"wmic.exe\"}]\nselection_cli: {\"CommandLine|contains\": \"process\"}\nfilter_main_creation: {\"CommandLine|contains|all\": [\"call\", \"create\"]}",
    "condition": "all of selection* and not 1 of filter_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
