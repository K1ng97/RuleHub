  "detection": {
    "query": "selection_key: [{\"TargetObject|contains\": \"\\\\System\\\\CurrentControlSet\\\\Services\"}, {\"TargetObject|contains|all\": [\"\\\\System\\\\ControlSet\", \"\\\\Services\"]}]\nselection_details: [{\"Details|contains|all\": [\"ADMIN$\", \".exe\"]}, {\"Details|contains|all\": [\"%COMSPEC%\", \"start\", \"powershell\"]}]",
    "condition": "all of selection_*",
    "fields": []
  },
