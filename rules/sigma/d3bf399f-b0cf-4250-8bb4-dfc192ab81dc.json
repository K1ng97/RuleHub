  "modified": "2022-05-16",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "lolbin: [{\"Image|endswith\": \"\\\\ie4uinit.exe\"}, {\"OriginalFileName\": \"IE4UINIT.EXE\"}]\nfilter_correct: {\"CurrentDirectory\": [\"c:\\\\windows\\\\system32\\\\\", \"c:\\\\windows\\\\sysWOW64\\\\\"]}\nfilter_missing: {\"CurrentDirectory\": null}",
    "condition": "lolbin and not 1 of filter_*",
    "fields": []
  },
  "falsepositives": [
    "ViberPC updater calls this binary with the following commandline \"ie4uinit.exe -ClearIconCache\""
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
