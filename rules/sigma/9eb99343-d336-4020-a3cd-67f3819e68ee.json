    "query": "selection: {\"EventID\": [4625, 4776], \"Status\": [\"0xC0000072\", \"0xC000006F\", \"0xC0000070\", \"0xC0000413\", \"0xC000018C\", \"0xC000015B\"]}\nfilter: {\"SubjectUserSid\": \"S-1-0-0\"}",
    "condition": "selection and not filter",
    "fields": []
  },
