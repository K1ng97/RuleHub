  "detection": {
    "query": "selection: {\"ContextInfo|contains|all\": [\" = ServerRemoteHost \", \"wsmprovhost.exe\"]}\nfilter_pwsh_archive: {\"ContextInfo|contains\": \"\\\\Windows\\\\system32\\\\WindowsPowerShell\\\\v1.0\\\\Modules\\\\Microsoft.PowerShell.Archive\\\\Microsoft.PowerShell.Archive.psm1\"}",
    "condition": "selection and not 1 of filter_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate use remote PowerShell sessions"
