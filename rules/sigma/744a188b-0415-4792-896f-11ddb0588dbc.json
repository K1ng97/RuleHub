  },
  "tags": [
    "attackdefense-evasion",
    "attackt1055"
  ],
  "author": "Alexander McDonald",
  "references": [
    "https://www.microsoft.com/security/blog/2021/12/09/a-closer-look-at-qakbots-latest-building-blocks-and-how-to-knock-them-down/",
    "https://www.fortinet.com/content/dam/fortinet/assets/analyst-reports/ar-qakbot.pdf"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2022-06-24",
  "modified": "2023-02-03",
  "mitre": {
    "tactics": [
      "TA1055"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"ParentImage|endswith\": \"\\\\msra.exe\", \"ParentCommandLine|endswith\": \"msra.exe\", \"Image|endswith\": [\"\\\\arp.exe\", \"\\\\cmd.exe\", \"\\\\net.exe\", \"\\\\netstat.exe\", \"\\\\nslookup.exe\", \"\\\\route.exe\", \"\\\\schtasks.exe\", \"\\\\whoami.exe\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Legitimate use of Msra.exe"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
