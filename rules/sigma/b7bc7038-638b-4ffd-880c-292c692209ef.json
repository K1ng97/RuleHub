  },
  "tags": [
    "attackpersistence",
    "attackt1505003"
  ],
  "author": "Max Altgelt (Nextron Systems)",
  "references": [
    "https://twitter.com/GossiTheDog/status/1429175908905127938"
  ],
  "severity": "critical",
  "type": "sigma",
  "status": "test",
  "created": "2021-08-23",
  "modified": "2023-01-23",
  "mitre": {
    "tactics": [
      "TA1505"
    ],
    "techniques": [
      "T003"
    ]
  },
  "detection": {
    "query": "keywords_export_command: {\"|all\": [\"New-ExchangeCertificate\", \" -GenerateRequest\", \" -BinaryEncoded\", \" -RequestFile\"]}\nkeywords_export_params: [\"\\\\\\\\\\\\\\\\localhost\\\\\\\\C$\", \"\\\\\\\\\\\\\\\\127.0.0.1\\\\\\\\C$\", \"C:\\\\\\\\inetpub\", \".aspx\"]",
    "condition": "keywords_export_command and keywords_export_params",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
  "level": "critical",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "msexchange-management"
  ]
