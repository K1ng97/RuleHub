  "id": "06b401f4-107c-4ff9-947f-9ec1e7649f1e",
  "name": "Potential Arbitrary Command Execution Via FTP.EXE",
  "description": "Detects execution of \"ftp.exe\" script with the \"-s\" or \"/s\" flag and any child processes ran by \"ftp.exe\".",
  "source": {
    "type": "sigma",
    "id": "06b401f4-107c-4ff9-947f-9ec1e7649f1e",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_ftp_arbitrary_command_execution.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_ftp_arbitrary_command_execution.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackexecution",
    "attackt1059",
    "attackt1202"
  ],
  "author": "Victor Sergeev, oscd.community",
  "references": [
    "https://lolbas-project.github.io/lolbas/Binaries/Ftp/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2020-10-09",
  "modified": "2024-04-23",
  "mitre": {
    "tactics": [
      "TA1059",
      "TA1202"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_parent: {\"ParentImage|endswith\": \"\\\\ftp.exe\"}\nselection_child_img: [{\"Image|endswith\": \"\\\\ftp.exe\"}, {\"OriginalFileName\": \"ftp.exe\"}]\nselection_child_cli: {\"CommandLine|contains|windash\": \"-s:\"}",
    "condition": "selection_parent or all of selection_child_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}