  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\powershell.exe\"}, {\"OriginalFileName\": \"PowerShell.EXE\"}]\nselection_cli: {\"CommandLine|contains|all\": [\"Set-Service\", \"-StartupType\"], \"CommandLine|contains\": [\"Disabled\", \"Manual\"]}",
    "condition": "all of selection_*",
