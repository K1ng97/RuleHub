  "references": [
    "https://web.archive.org/web/20220306121156/https://www.x86matthew.com/view_post?id=ntdll_pipe"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2022-03-05",
  "modified": "2023-03-07",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"CommandLine|contains\": [\"type %windir%\\\\system32\\\\ntdll.dll\", \"type %systemroot%\\\\system32\\\\ntdll.dll\", \"type c:\\\\windows\\\\system32\\\\ntdll.dll\", \"\\\\\\\\ntdll.dll > \\\\\\\\\\\\\\\\.\\\\\\\\pipe\\\\\\\\\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
