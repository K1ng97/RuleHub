  },
  "tags": [
    "attackdefense-evasion",
    "attackt1218"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://twitter.com/0xBoku/status/1679200664013135872"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2023-08-03",
  "modified": "2023-08-03",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\VSDiagnostics.exe\"}, {\"OriginalFileName\": \"VSDiagnostics.exe\"}]\nselection_cli_start: {\"CommandLine|contains\": \"start\"}\nselection_cli_launch: {\"CommandLine|contains\": [\" /launch:\", \" -launch:\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate usage for tracing and diagnostics purposes"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}