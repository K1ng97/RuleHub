{
  "id": "7047d730-036f-4f40-b9d8-1c63e36d5e62",
  "name": "Potential Binary Or Script Dropper Via PowerShell",
  "description": "Detects PowerShell creating a binary executable or a script file.",
  "source": {
    "type": "sigma",
    "id": "7047d730-036f-4f40-b9d8-1c63e36d5e62",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/file_event_win_powershell_drop_binary_or_script.yml",
    "file_path": "tmp/repos/sigma/rules/windows/file/file_event/file_event_win_powershell_drop_binary_or_script.yml"
  },
  "tags": [
    "attackpersistence"
  ],
  "author": "frack113, Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://www.zscaler.com/blogs/security-research/onenote-growing-threat-malware-distribution"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2023-03-17",
  "modified": "2025-03-05",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": [\"\\\\powershell.exe\", \"\\\\pwsh.exe\"], \"TargetFilename|endswith\": [\".bat\", \".chm\", \".cmd\", \".com\", \".dll\", \".exe\", \".hta\", \".jar\", \".js\", \".ocx\", \".scr\", \".sys\", \".vbe\", \".vbs\", \".wsf\"]}\nfilter_main_user_temp: {\"TargetFilename|startswith\": \"C:\\\\Users\\\\\", \"TargetFilename|contains\": \"\\\\AppData\\\\Local\\\\Temp\\\\\", \"TargetFilename|endswith\": [\".dll\", \".exe\"]}\nfilter_main_other_temp: {\"TargetFilename|startswith\": [\"C:\\\\Windows\\\\Temp\\\\\", \"C:\\\\Windows\\\\SystemTemp\\\\\"], \"TargetFilename|endswith\": [\".dll\", \".exe\"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "False positives will differ depending on the environment and scripts used. Apply additional filters accordingly."
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "file_event"
  ]
}