  "detection": {
    "query": "selection_wscript_img: [{\"Image|endswith\": \"\\\\wscript.exe\"}, {\"OriginalFileName\": \"wscript.exe\"}]\nselection_wscript_cli: {\"CommandLine|contains\": \"manage-bde.wsf\"}\nselection_parent: {\"ParentImage|endswith\": [\"\\\\cscript.exe\", \"\\\\wscript.exe\"], \"ParentCommandLine|contains\": \"manage-bde.wsf\"}\nselection_filter_cmd: {\"Image|endswith\": \"\\\\cmd.exe\"}",
    "condition": "all of selection_wscript_* or (selection_parent and not selection_filter_cmd)",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
