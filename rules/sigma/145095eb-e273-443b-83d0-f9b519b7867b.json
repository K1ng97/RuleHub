  "created": "2024-07-08",
  "modified": "2024-07-08",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\regedit.exe\", \"TargetFilename|endswith\": \".pdf\"}",
    "condition": "selection",
    "fields": []
