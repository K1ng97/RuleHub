  "mitre": {
    "tactics": [
      "TA1505"
    ],
    "techniques": [
      "T003"
    ]
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\w3wp.exe\", \"CommandLine|contains\": \"MSExchange\", \"TargetFilename|contains\": [\"FrontEnd\\\\HttpProxy\\\\\", \"\\\\inetpub\\\\wwwroot\\\\aspnet_client\\\\\"]}\nselection_types: {\"TargetFilename|endswith\": [\".aspx\", \".asp\", \".ashx\"]}",
    "condition": "all of selection*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "file_event"
  ]
}