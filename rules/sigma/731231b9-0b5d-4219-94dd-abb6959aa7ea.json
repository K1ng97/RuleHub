    "attackt1218011"
  ],
  "author": "Florian Roth (Nextron Systems)",
  "references": [
    "https://www.microsoft.com/security/blog/2021/03/04/goldmax-goldfinder-sibot-analyzing-nobelium-malware/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2021-03-05",
  "modified": "2022-10-09",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": [
      "T011"
    ]
  },
  "detection": {
    "query": "selection1: {\"CommandLine|contains\": \"rundll32.exe\"}\nselection2: {\"CommandLine|contains\": [\".sys,\", \".sys \"]}",
    "condition": "all of selection*",
    "fields": []
  },
  "falsepositives": [
