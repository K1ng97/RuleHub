{
  "id": "cfeed607-6aa4-4bbd-9627-b637deb723c8",
  "name": "New or Renamed User Account with '$' Character",
  "description": "Detects the creation of a user with the \"$\" character. This can be used by attackers to hide a user or trick detection systems that lack the parsing mechanisms.\n",
  "source": {
    "type": "sigma",
    "id": "cfeed607-6aa4-4bbd-9627-b637deb723c8",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/win_security_new_or_renamed_user_account_with_dollar_sign.yml",
    "file_path": "tmp/repos/sigma/rules/windows/builtin/security/win_security_new_or_renamed_user_account_with_dollar_sign.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1036"
  ],
  "author": "Ilyas Ochkov, oscd.community",
  "references": [
    "https://twitter.com/SBousseaden/status/1387743867663958021"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2019-10-25",
  "modified": "2024-01-16",
  "mitre": {
    "tactics": [
      "TA1036"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_create: {\"EventID\": 4720, \"SamAccountName|contains\": \"$\"}\nselection_rename: {\"EventID\": 4781, \"NewTargetUserName|contains\": \"$\"}\nfilter_main_homegroup: {\"EventID\": 4720, \"TargetUserName\": \"HomeGroupUser$\"}",
    "condition": "1 of selection_* and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "security"
  ]
}