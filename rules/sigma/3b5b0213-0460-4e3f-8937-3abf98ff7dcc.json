  "name": "Suspicious Workstation Locking via Rundll32",
  "description": "Detects a suspicious call to the user32.dll function that locks the user workstation",
  "source": {
    "type": "sigma",
    "id": "3b5b0213-0460-4e3f-8937-3abf98ff7dcc",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_rundll32_user32_dll.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_rundll32_user32_dll.yml"
  },
  "tags": [
    "attackdefense-evasion"
  ],
  "author": "frack113",
  "references": [
    "https://app.any.run/tasks/2aef9c63-f944-4763-b3ef-81eee209d128/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-06-04",
  "modified": "2023-02-09",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection_call_img: [{\"Image|endswith\": \"\\\\rundll32.exe\"}, {\"OriginalFileName\": \"RUNDLL32.EXE\"}]\nselection_call_parent: {\"ParentImage|endswith\": \"\\\\cmd.exe\"}\nselection_call_cli: {\"CommandLine|contains\": \"user32.dll,\"}\nselection_function: {\"CommandLine|contains\": \"LockWorkStation\"}",
    "condition": "all of selection_*",
    "fields": [
      "Image",
      "ParentImage"
    ]
  },
  "falsepositives": [
    "Scripts or links on the user desktop used to lock the workstation instead of Windows+L or the menu option"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}