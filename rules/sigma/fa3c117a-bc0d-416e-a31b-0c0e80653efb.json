  "detection": {
    "query": "selection_origin: [{\"Image|endswith\": \"\\\\w3wp.exe\"}, {\"ParentImage|endswith\": \"\\\\w3wp.exe\"}]\nselection_cmdline: {\"CommandLine|contains\": [\"&ipconfig&echo\", \"&quser&echo\", \"&whoami&echo\", \"&c:&echo\", \"&cd&echo\", \"&dir&echo\", \"&echo [E]\", \"&echo [S]\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
