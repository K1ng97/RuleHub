  "mitre": {
    "tactics": [
      "TA1055"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_malleable_profile_generic: [{\"PipeName|startswith\": [\"\\\\DserNamePipe\", \"\\\\f4c3\", \"\\\\f53f\", \"\\\\fullduplex_\", \"\\\\mojo.5688.8052.183894939787088877\", \"\\\\mojo.5688.8052.35780273329370473\", \"\\\\MsFteWds\", \"\\\\msrpc_\", \"\\\\mypipe-f\", \"\\\\mypipe-h\", \"\\\\ntsvcs\", \"\\\\PGMessagePipe\", \"\\\\rpc_\", \"\\\\scerpc\", \"\\\\SearchTextHarvester\", \"\\\\spoolss\", \"\\\\win_svc\", \"\\\\win\\\\msrpc_\", \"\\\\windows.update.manager\", \"\\\\wkssvc\"]}, {\"PipeName\": [\"\\\\demoagent_11\", \"\\\\demoagent_22\"]}]\nselection_malleable_profile_catalog_change_listener: {\"PipeName|startswith\": \"\\\\Winsock2\\\\CatalogChangeListener-\", \"PipeName|endswith\": \"-0,\"}\nfilter_main_generic: {\"PipeName\": [\"\\\\wkssvc\", \"\\\\spoolss\", \"\\\\scerpc\", \"\\\\ntsvcs\", \"\\\\SearchTextHarvester\", \"\\\\PGMessagePipe\", \"\\\\MsFteWds\"]}\nfilter_optional_websense: {\"Image|contains\": [\":\\\\Program Files\\\\Websense\\\\\", \":\\\\Program Files (x86)\\\\Websense\\\\\"], \"PipeName|startswith\": [\"\\\\DserNamePipeR\", \"\\\\DserNamePipeW\"]}",
    "condition": "1 of selection_malleable_profile_* and not 1 of filter_main_* and not 1 of filter_optional_*",
    "fields": []
  },
  "falsepositives": [
    "Chrome instances using the exact same pipe name \"mojo.xxx\"",
    "Websense Endpoint using the pipe name \"DserNamePipe(R|W)\\d{1,5}\""
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "pipe_created"
  ]
}