  "detection": {
    "query": "selection: {\"ImageLoaded|endswith\": \"\\\\chrome_frame_helper.dll\"}\nfilter_main_path: {\"ImageLoaded|startswith\": [\"C:\\\\Program Files\\\\Google\\\\Chrome\\\\Application\\\\\", \"C:\\\\Program Files (x86)\\\\Google\\\\Chrome\\\\Application\\\\\"]}\nfilter_optional_user_path: {\"ImageLoaded|contains\": \"\\\\AppData\\\\local\\\\Google\\\\Chrome\\\\Application\\\\\"}",
    "condition": "selection and not 1 of filter_main_* and not 1 of filter_optional_*",
    "fields": []
  },
  "falsepositives": [
