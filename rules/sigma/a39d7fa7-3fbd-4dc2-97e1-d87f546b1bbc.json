  "detection": {
    "query": "selection: {\"type\": \"SYSCALL\", \"exe|startswith\": [\"/tmp/\", \"/var/www/\", \"/home/*/public_html/\", \"/usr/local/apache2/\", \"/usr/local/httpd/\", \"/var/apache/\", \"/srv/www/\", \"/home/httpd/html/\", \"/srv/http/\", \"/usr/share/nginx/html/\", \"/var/lib/pgsql/data/\", \"/usr/local/mysql/data/\", \"/var/lib/mysql/\", \"/var/vsftpd/\", \"/etc/bind/\", \"/var/named/\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Admin activity (especially in /tmp folders)",
    "Crazy web applications"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "linux",
    "auditd"
  ]
