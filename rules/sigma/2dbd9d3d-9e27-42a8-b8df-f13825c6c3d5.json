    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_rundll32_webdav_client_execution.yml"
  },
  "tags": [
    "attackexfiltration",
    "attackt1048003"
  ],
  "author": "Roberto Rodriguez (Cyb3rWard0g), OTR (Open Threat Research)",
  "references": [
    "https://github.com/OTRF/detection-hackathon-apt29/issues/17",
    "https://github.com/OTRF/ThreatHunter-Playbook/blob/2d4257f630f4c9770f78d0c1df059f891ffc3fec/docs/evals/apt29/detections/7.B.4_C10730EA-6345-4934-AA0F-B0EFCA0C4BA6.md"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2020-05-02",
  "modified": "2023-09-18",
  "mitre": {
    "tactics": [
      "TA1048"
    ],
    "techniques": [
      "T003"
    ]
  },
  "detection": {
    "query": "selection_parent: {\"ParentImage|endswith\": \"\\\\svchost.exe\"}\nselection_img: [{\"Image|endswith\": \"\\\\rundll32.exe\"}, {\"OriginalFileName\": \"RUNDLL32.EXE\"}]\nselection_cli: {\"CommandLine|contains\": \"C:\\\\windows\\\\system32\\\\davclnt.dll,DavSetCookie\"}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
