  "status": "test",
  "created": "2022-09-20",
  "modified": "2023-02-14",
  "mitre": {
    "tactics": [
      "TA1003",
      "TA1036"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection_pe: {\"OriginalFileName\": \"FX_VER_INTERNALNAME_STR\"}\nselection_cli: [{\"CommandLine|contains|all\": [\" -u \", \" -f \", \".dmp\"]}, {\"CommandLine|contains|all\": [\" --full \", \" --name \", \".dmp\"]}]\nfilter: {\"Image|endswith\": \"\\\\createdump.exe\"}",
    "condition": "1 of selection_* and not filter",
    "fields": []
  },
  "falsepositives": [
    "Command lines that use the same flags"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}