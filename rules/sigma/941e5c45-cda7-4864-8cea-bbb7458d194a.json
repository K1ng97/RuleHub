  "detection": {
    "query": "selection: {\"EventID\": 4648, \"ProcessName|endswith\": [\"\\\\cmd.exe\", \"\\\\powershell.exe\", \"\\\\pwsh.exe\", \"\\\\winrs.exe\", \"\\\\wmic.exe\", \"\\\\net.exe\", \"\\\\net1.exe\", \"\\\\reg.exe\"]}\nfilter1: {\"TargetServerName\": \"localhost\"}\nfilter2: {\"SubjectUserName|endswith\": \"$\", \"TargetUserName|endswith\": \"$\"}",
    "condition": "selection and not 1 of filter*",
    "fields": []
  },
  "falsepositives": [
    "Administrators that use the RunAS command or scheduled tasks"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
