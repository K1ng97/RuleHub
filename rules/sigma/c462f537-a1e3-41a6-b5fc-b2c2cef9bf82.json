    "file_path": "tmp/repos/sigma/rules/windows/builtin/security/win_security_susp_psexec.yml"
  },
  "tags": [
    "attacklateral-movement",
    "attackt1021002"
  ],
  "author": "Samir Bousseaden",
  "references": [
    "https://web.archive.org/web/20230329171218/https://blog.menasec.net/2019/02/threat-hunting-3-detecting-psexec.html"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2019-04-03",
  "modified": "2022-08-11",
  "mitre": {
    "tactics": [
      "TA1021"
    ],
    "techniques": [
      "T002"
    ]
  },
  "detection": {
    "query": "selection1: {\"EventID\": 5145, \"ShareName\": \"\\\\\\\\\\\\\\\\\\\\*\\\\\\\\IPC$\", \"RelativeTargetName|endswith\": [\"-stdin\", \"-stdout\", \"-stderr\"]}\nfilter: {\"RelativeTargetName|startswith\": \"PSEXESVC\"}",
    "condition": "selection1 and not filter",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "security"
  ]
}