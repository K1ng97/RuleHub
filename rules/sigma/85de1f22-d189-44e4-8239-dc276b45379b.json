{
  "id": "85de1f22-d189-44e4-8239-dc276b45379b",
  "name": "Curl Web Request With Potential Custom User-Agent",
  "description": "Detects execution of \"curl.exe\" with a potential custom \"User-Agent\". Attackers can leverage this to download or exfiltrate data via \"curl\" to a domain that only accept specific \"User-Agent\" strings",
  "source": {
    "type": "sigma",
    "id": "85de1f22-d189-44e4-8239-dc276b45379b",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_curl_custom_user_agent.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_curl_custom_user_agent.yml"
  },
  "tags": [
    "attackexecution"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://labs.withsecure.com/publications/fin7-target-veeam-servers",
    "https://github.com/WithSecureLabs/iocs/blob/344203de742bb7e68bd56618f66d34be95a9f9fc/FIN7VEEAM/iocs.csv"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2023-07-27",
  "modified": "2023-07-27",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\curl.exe\"}, {\"OriginalFileName\": \"curl.exe\"}]\nselection_header: {\"CommandLine|re\": \"\\\\s-H\\\\s\", \"CommandLine|contains\": \"User-Agent:\"}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}