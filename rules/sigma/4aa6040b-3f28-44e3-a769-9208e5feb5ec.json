  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": [
      "T011"
    ]
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\rundll32.exe\"}, {\"OriginalFileName\": \"RUNDLL32.exe\"}]\nselection_cli: {\"CommandLine|contains\": [\".bmp\", \".cr2\", \".eps\", \".gif\", \".ico\", \".jpeg\", \".jpg\", \".nef\", \".orf\", \".png\", \".raw\", \".sr2\", \".tif\", \".tiff\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
