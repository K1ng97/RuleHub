  "name": "Java Running with Remote Debugging",
  "description": "Detects a JAVA process running with remote debugging allowing more than just localhost to connect",
  "source": {
    "type": "sigma",
    "id": "8f88e3f6-2a49-48f5-a5c4-2f7eedf78710",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_java_remote_debugging.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_java_remote_debugging.yml"
  },
  "tags": [
    "attackexecution",
    "attackt1203"
  ],
  "author": "Florian Roth (Nextron Systems)",
  "references": [
    "https://dzone.com/articles/remote-debugging-java-applications-with-jdwp"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2019-01-16",
  "modified": "2023-02-01",
  "mitre": {
    "tactics": [
      "TA1203"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_jdwp_transport: {\"CommandLine|contains\": \"transport=dt_socket,address=\"}\nselection_old_jvm_version: {\"CommandLine|contains\": [\"jre1.\", \"jdk1.\"]}\nexclusion: {\"CommandLine|contains\": [\"address=127.0.0.1\", \"address=localhost\"]}",
    "condition": "all of selection_* and not exclusion",
    "fields": [
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}