  "detection": {
    "query": "selection: {\"CommandLine|contains\": [\"[System.Net.WebRequest]::create\", \"curl \", \"Invoke-RestMethod\", \"Invoke-WebRequest\", \"iwr \", \"Net.WebClient\", \"Resume-BitsTransfer\", \"Start-BitsTransfer\", \"wget \", \"WinHttp.WinHttpRequest\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Use of Get-Command and Get-Help modules to reference Invoke-WebRequest and Start-BitsTransfer."
