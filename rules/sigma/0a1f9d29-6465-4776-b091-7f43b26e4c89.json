  "detection": {
    "query": "selection: {\"TargetFilename|contains\": \":\\\\Windows\\\\Prefetch\\\\\", \"TargetFilename|endswith\": \".pf\"}\nfilter_main_svchost: {\"Image|endswith\": \":\\\\windows\\\\system32\\\\svchost.exe\", \"User|contains\": [\"AUTHORI\", \"AUTORI\"]}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
