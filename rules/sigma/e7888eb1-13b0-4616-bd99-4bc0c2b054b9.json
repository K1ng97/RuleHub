  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\dllhost.exe\", \"CommandLine\": [\"dllhost.exe\", \"dllhost\"]}\nfilter_main_null: {\"CommandLine\": null}",
    "condition": "selection and not 1 of filter_main_*",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
