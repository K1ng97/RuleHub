  "id": "044ba588-dff4-4918-9808-3f95e8160606",
  "name": "Copy .DMP/.DUMP Files From Remote Share Via Cmd.EXE",
  "description": "Detects usage of the copy builtin cmd command to copy files with the \".dmp\"/\".dump\" extension from a remote share",
  "source": {
    "type": "sigma",
    "id": "044ba588-dff4-4918-9808-3f95e8160606",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/proc_creation_win_cmd_copy_dmp_from_share.yml",
    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_cmd_copy_dmp_from_share.yml"
  },
  "tags": [
    "attackcredential-access"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://thedfirreport.com/2022/09/26/bumblebee-round-two/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2022-09-27",
  "modified": "2023-09-12",
  "mitre": {
    "tactics": [],
    "techniques": []
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\cmd.exe\"}, {\"OriginalFileName\": \"Cmd.Exe\"}]\nselection_cli: {\"CommandLine|contains|all\": [\"copy \", \" \\\\\\\\\\\\\\\\\"], \"CommandLine|contains\": [\".dmp\", \".dump\", \".hdmp\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
