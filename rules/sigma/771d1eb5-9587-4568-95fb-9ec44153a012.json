  "detection": {
    "query": "selection_img: [{\"Image|endswith\": [\"\\\\NSudo.exe\", \"\\\\NSudoLC.exe\", \"\\\\NSudoLG.exe\"]}, {\"OriginalFileName\": [\"NSudo.exe\", \"NSudoLC.exe\", \"NSudoLG.exe\"]}]\nselection_cli: {\"CommandLine|contains\": [\"-U:S \", \"-U:T \", \"-U:E \", \"-P:E \", \"-M:S \", \"-M:H \", \"-U=S \", \"-U=T \", \"-U=E \", \"-P=E \", \"-M=S \", \"-M=H \", \"-ShowWindowMode:Hide\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate use by administrators"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
