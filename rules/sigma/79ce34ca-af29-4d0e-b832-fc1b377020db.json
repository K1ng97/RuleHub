  "detection": {
    "query": "selection_img: [{\"OriginalFileName\": \"whoami.exe\"}, {\"Image|endswith\": \"\\\\whoami.exe\"}]\nselection_user: {\"User|contains\": [\"AUTHORI\", \"AUTORI\", \"TrustedInstaller\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
