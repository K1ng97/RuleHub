    "file_path": "tmp/repos/sigma/rules/linux/auditd/lnx_auditd_user_discovery.yml"
  },
  "tags": [
    "attackdiscovery",
    "attackt1033"
  ],
  "author": "Timur Zinniatullin, oscd.community",
  "references": [
    "https://github.com/redcanaryco/atomic-red-team/blob/f339e7da7d05f6057fdfcdd3742bfcf365fee2a9/atomics/T1033/T1033.md"
  ],
  "severity": "low",
  "type": "sigma",
  "status": "test",
  "created": "2019-10-21",
  "modified": "2024-11-30",
  "mitre": {
    "tactics": [
      "TA1033"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection: {\"type\": \"EXECVE\", \"a0\": [\"hostname\", \"id\", \"last\", \"users\", \"w\", \"who\", \"whoami\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Admin activity"
  ],
  "level": "low",
  "rule_format": "standard",
  "platforms": [],
