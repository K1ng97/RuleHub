  "detection": {
    "query": "selection_root: {\"Provider_Name\": \"Service Control Manager\", \"EventID\": 7045}\nselection_service: [{\"ImagePath|contains\": \"PDQDeployService.exe\"}, {\"ServiceName\": [\"PDQDeploy\", \"PDQ Deploy\"]}]",
    "condition": "all of selection_*",
    "fields": []
  },
