  "detection": {
    "query": "selection_name: {\"Image|endswith\": \"\\\\SharpLdapWhoami.exe\"}\nselection_pe: [{\"OriginalFileName|contains\": \"SharpLdapWhoami\"}, {\"Product\": \"SharpLdapWhoami\"}]\nselection_flags1: {\"CommandLine|endswith\": [\" /method:ntlm\", \" /method:kerb\", \" /method:nego\", \" /m:nego\", \" /m:ntlm\", \" /m:kerb\"]}",
    "condition": "1 of selection*",
    "fields": []
  },
  "falsepositives": [
    "Programs that use the same command line flags"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
