  "detection": {
    "query": "session_manager_base: {\"TargetObject|contains\": \"\\\\System\\\\CurrentControlSet\\\\Control\\\\Session Manager\"}\nsession_manager: {\"TargetObject|contains\": [\"\\\\SetupExecute\", \"\\\\S0InitialCommand\", \"\\\\KnownDlls\", \"\\\\Execute\", \"\\\\BootExecute\", \"\\\\AppCertDlls\"]}\nfilter: {\"Details\": \"(Empty)\"}",
    "condition": "session_manager_base and session_manager and not filter",
    "fields": [
      "SecurityID",
      "ObjectName",
      "OldValueType",
      "NewValueType"
    ]
  },
  "falsepositives": [
    "Legitimate software automatically (mostly, during installation) sets up autorun keys for legitimate reason",
    "Legitimate administrator sets up autorun keys for legitimate reason"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
