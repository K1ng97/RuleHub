  "detection": {
    "query": "selection_ad_principal: [{\"Payload|contains\": \"get-ADPrincipalGroupMembership\"}, {\"ContextInfo|contains\": \"get-ADPrincipalGroupMembership\"}]\nselection_get_aduser: [{\"Payload|contains|all\": [\"get-aduser\", \"-f \", \"-pr \", \"DoesNotRequirePreAuth\"]}, {\"ContextInfo|contains|all\": [\"get-aduser\", \"-f \", \"-pr \", \"DoesNotRequirePreAuth\"]}]",
    "condition": "1 of selection_*",
