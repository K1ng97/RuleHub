  "mitre": {
    "tactics": [
      "TA1204"
    ],
    "techniques": [
      "T002"
    ]
  },
  "detection": {
    "query": "selection1: {\"Image|endswith\": [\"\\\\excel.exe\", \"\\\\msaccess.exe\", \"\\\\mspub.exe\", \"\\\\powerpnt.exe\", \"\\\\visio.exe\", \"\\\\winword.exe\"]}\nselection2: {\"TargetFilename|endswith\": [\".bat\", \".cmd\", \".com\", \".dll\", \".exe\", \".hta\", \".ocx\", \".proj\", \".ps1\", \".scf\", \".scr\", \".sys\", \".vbe\", \".vbs\", \".wsf\", \".wsh\"]}\nfilter_optional_webservicecache: {\"TargetFilename|contains|all\": [\"C:\\\\Users\\\\\", \"\\\\AppData\\\\Local\\\\Microsoft\\\\Office\\\\\", \"\\\\WebServiceCache\\\\AllUsers\"], \"TargetFilename|endswith\": \".com\"}\nfilter_optional_webex: {\"Image|endswith\": \"\\\\winword.exe\", \"TargetFilename|contains\": \"\\\\AppData\\\\Local\\\\Temp\\\\webexdelta\\\\\", \"TargetFilename|endswith\": [\".dll\", \".exe\"]}\nfilter_main_localassembly: {\"TargetFilename|contains\": \"\\\\AppData\\\\Local\\\\assembly\\\\tmp\\\\\", \"TargetFilename|endswith\": \".dll\"}",
    "condition": "all of selection* and not 1 of filter_main_* and not 1 of filter_optional_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "file_event"
  ]
}