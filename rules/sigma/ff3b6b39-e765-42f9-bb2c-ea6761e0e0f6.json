  "detection": {
    "query": "selection_parent: {\"ParentImage|endswith\": [\"\\\\brave.exe\", \"\\\\CCleanerBrowser.exe\", \"\\\\chrome.exe\", \"\\\\chromium.exe\", \"\\\\firefox.exe\", \"\\\\iexplore.exe\", \"\\\\microsoftedge.exe\", \"\\\\msedge.exe\", \"\\\\opera.exe\", \"\\\\vivaldi.exe\", \"\\\\whale.exe\", \"\\\\outlook.exe\"]}\nselection_img: [{\"Image|endswith\": \"\\\\mstsc.exe\"}, {\"OriginalFileName\": \"mstsc.exe\"}]",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
