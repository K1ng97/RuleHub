  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\aspnet_compiler.exe\", \"TargetFilename|contains|all\": [\"\\\\Temporary ASP.NET Files\\\\\", \"\\\\assembly\\\\tmp\\\\\", \".dll\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Legitimate assembly compilation using a build provider"
  ],
