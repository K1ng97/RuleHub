  "detection": {
    "query": "selection: {\"EventID\": 16403, \"LocalName|endswith\": [\".bat\", \".dll\", \".exe\", \".hta\", \".ps1\", \".psd1\", \".sh\", \".vbe\", \".vbs\"]}\nfilter_optional_generic: {\"LocalName|contains\": \"\\\\AppData\\\\\", \"RemoteName|contains\": \".com\"}",
    "condition": "selection and not 1 of filter_optional_*",
