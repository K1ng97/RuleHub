    "query": "selection: [{\"Image|endswith\": \"\\\\msiexec.exe\"}, {\"OriginalFileName\": \"\\\\msiexec.exe\"}]\nfilter: {\"Image|startswith\": [\"C:\\\\Windows\\\\System32\\\\\", \"C:\\\\Windows\\\\SysWOW64\\\\\", \"C:\\\\Windows\\\\WinSxS\\\\\"]}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
