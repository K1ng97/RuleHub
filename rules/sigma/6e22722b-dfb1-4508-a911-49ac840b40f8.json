  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\mstsc.exe\"}, {\"OriginalFileName\": \"mstsc.exe\"}]\nselection_extension: {\"CommandLine|endswith\": [\".rdp\", \".rdp\\\"\"]}\nselection_paths: {\"CommandLine|contains\": [\":\\\\Users\\\\Public\\\\\", \":\\\\Windows\\\\System32\\\\spool\\\\drivers\\\\color\", \":\\\\Windows\\\\System32\\\\Tasks_Migrated \", \":\\\\Windows\\\\Tasks\\\\\", \":\\\\Windows\\\\Temp\\\\\", \":\\\\Windows\\\\Tracing\\\\\", \"\\\\AppData\\\\Local\\\\Temp\\\\\", \"\\\\Downloads\\\\\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Likelihood is related to how often the paths are used in the environment"
  ],
