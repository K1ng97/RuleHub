  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2022-12-29",
  "modified": "2023-01-25",
  "mitre": {
    "tactics": [
      "TA1218"
    ],
    "techniques": []
  },
  "detection": {
    "query": "selection_parent: {\"ParentImage\": \"C:\\\\Windows\\\\System32\\\\OpenSSH\\\\sshd.exe\"}\nselection_cli_img: {\"Image|endswith\": \"\\\\ssh.exe\"}\nselection_cli_flags: [{\"CommandLine|contains\": \"ProxyCommand=\"}, {\"CommandLine|contains|all\": [\"PermitLocalCommand\", \"LocalCommand\"]}]",
    "condition": "selection_parent or all of selection_cli_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate usage for administration purposes"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
}