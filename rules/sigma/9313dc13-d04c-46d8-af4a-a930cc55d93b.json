{
  "id": "9313dc13-d04c-46d8-af4a-a930cc55d93b",
  "name": "Potential DLL Sideloading Via VMware Xfer",
  "description": "Detects loading of a DLL by the VMware Xfer utility from the non-default directory which may be an attempt to sideload arbitrary DLL",
  "source": {
    "type": "sigma",
    "id": "9313dc13-d04c-46d8-af4a-a930cc55d93b",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/image_load_side_load_vmware_xfer.yml",
    "file_path": "tmp/repos/sigma/rules/windows/image_load/image_load_side_load_vmware_xfer.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1574001"
  ],
  "author": "Nasreddine Bencherchali (Nextron Systems)",
  "references": [
    "https://www.sentinelone.com/labs/lockbit-ransomware-side-loads-cobalt-strike-beacon-with-legitimate-vmware-utility/"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2022-08-02",
  "modified": "2023-02-17",
  "mitre": {
    "tactics": [
      "TA1574"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\VMwareXferlogs.exe\", \"ImageLoaded|endswith\": \"\\\\glib-2.0.dll\"}\nfilter: {\"ImageLoaded|startswith\": \"C:\\\\Program Files\\\\VMware\\\\\"}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "Unlikely"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "image_load"
  ]
}