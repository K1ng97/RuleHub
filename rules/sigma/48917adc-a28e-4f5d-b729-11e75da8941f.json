  "detection": {
    "query": "selection: {\"Image|endswith\": \"\\\\reg.exe\", \"CommandLine|contains\": [\"SOFTWARE\\\\Microsoft\\\\Windows Defender\\\\Exclusions\\\\Paths\", \"SOFTWARE\\\\Microsoft\\\\Microsoft Antimalware\\\\Exclusions\\\\Paths\"], \"CommandLine|contains|all\": [\"ADD \", \"/t \", \"REG_DWORD \", \"/v \", \"/d \", \"0\"]}",
    "condition": "selection",
    "fields": []
  },
