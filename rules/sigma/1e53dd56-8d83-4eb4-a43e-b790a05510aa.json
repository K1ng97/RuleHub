    "file_path": "tmp/repos/sigma/rules/windows/process_creation/proc_creation_win_susp_elavated_msi_spawned_shell.yml"
  },
  "tags": [
    "attackprivilege-escalation",
    "attackt1548002"
  ],
  "author": "Teymur Kheirkhabarov (idea), Mangatas Tondang (rule), oscd.community",
  "references": [
    "https://image.slidesharecdn.com/kheirkhabarovoffzonefinal-181117201458/95/hunting-for-privilege-escalation-in-windows-environment-50-638.jpg"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2020-10-13",
  "modified": "2022-10-20",
  "mitre": {
    "tactics": [
      "TA1548"
    ],
    "techniques": [
      "T002"
    ]
  },
  "detection": {
    "query": "selection_img: [{\"Image|endswith\": [\"\\\\cmd.exe\", \"\\\\powershell.exe\", \"\\\\pwsh.exe\"]}, {\"OriginalFileName\": [\"Cmd.Exe\", \"PowerShell.EXE\", \"pwsh.dll\"]}]\nselection_parent: {\"ParentImage|contains|all\": [\"\\\\Windows\\\\Installer\\\\\", \"msi\"], \"ParentImage|endswith\": \"tmp\"}",
    "condition": "all of selection_*",
    "fields": [
      "Image",
      "ParentImage"
    ]
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
