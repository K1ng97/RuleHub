    "file_path": "tmp/repos/sigma/rules/windows/process_access/proc_access_win_lsass_dump_keyword_image.yml"
  },
  "tags": [
    "attackcredential-access",
    "attacks0002",
    "attackt1003001"
  ],
  "author": "Florian Roth (Nextron Systems)",
  "references": [
    "https://twitter.com/_xpn_/status/1491557187168178176",
    "https://www.ired.team/offensive-security/credential-access-and-credential-dumping/dump-credentials-from-lsass-process-without-mimikatz"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2022-02-10",
  "modified": "2023-11-29",
  "mitre": {
    "tactics": [
      "TA1003"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"TargetImage|endswith\": \"\\\\lsass.exe\", \"SourceImage|contains\": \"dump\", \"GrantedAccess|endswith\": [\"10\", \"30\", \"50\", \"70\", \"90\", \"B0\", \"D0\", \"F0\", \"18\", \"38\", \"58\", \"78\", \"98\", \"B8\", \"D8\", \"F8\", \"1A\", \"3A\", \"5A\", \"7A\", \"9A\", \"BA\", \"DA\", \"FA\", \"0x14C2\", \"FF\"]}",
    "condition": "selection",
    "fields": []
  },
  "falsepositives": [
    "Rare programs that contain the word dump in their name and access lsass"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_access"
  ]
}