{
  "id": "6fb63b40-e02a-403e-9ffd-3bcc1d749442",
  "name": "Metasploit Or Impacket Service Installation Via SMB PsExec",
  "description": "Detects usage of Metasploit SMB PsExec (exploit/windows/smb/psexec) and Impacket psexec.py by triggering on specific service installation",
  "source": {
    "type": "sigma",
    "id": "6fb63b40-e02a-403e-9ffd-3bcc1d749442",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/win_security_metasploit_or_impacket_smb_psexec_service_install.yml",
    "file_path": "tmp/repos/sigma/rules/windows/builtin/security/win_security_metasploit_or_impacket_smb_psexec_service_install.yml"
  },
  "tags": [
    "attackexecution",
    "attacklateral-movement",
    "attackt1021002",
    "attackt1569002",
    "attackt1570"
  ],
  "author": "Bartlomiej Czyz, Relativity",
  "references": [
    "https://bczyz1.github.io/2021/01/30/psexec.html"
  ],
  "severity": "high",
  "type": "sigma",
  "status": "test",
  "created": "2021-01-21",
  "modified": "2022-10-05",
  "mitre": {
    "tactics": [
      "TA1021",
      "TA1569",
      "TA1570"
    ],
    "techniques": [
      "T002"
    ]
  },
  "detection": {
    "query": "selection: {\"EventID\": 4697, \"ServiceFileName|re\": \"^%systemroot%\\\\\\\\[a-zA-Z]{8}\\\\.exe$\", \"ServiceName|re\": \"(^[a-zA-Z]{4}$)|(^[a-zA-Z]{8}$)|(^[a-zA-Z]{16}$)\", \"ServiceStartType\": 3, \"ServiceType\": \"0x10\"}\nfilter: {\"ServiceName\": \"PSEXESVC\"}",
    "condition": "selection and not filter",
    "fields": [
      "ComputerName",
      "SubjectDomainName",
      "SubjectUserName",
      "ServiceName",
      "ServiceFileName"
    ]
  },
  "falsepositives": [
    "Possible, different agents with a 8 character binary and a 4, 8 or 16 character service name"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "security"
  ]
}