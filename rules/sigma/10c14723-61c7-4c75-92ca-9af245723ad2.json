  },
  "detection": {
    "query": "selection_other: {\"ParentImage|endswith\": [\"\\\\wmiprvse.exe\", \"\\\\mmc.exe\", \"\\\\explorer.exe\", \"\\\\services.exe\"], \"CommandLine|contains|all\": [\"cmd.exe\", \"/Q\", \"/c\", \"\\\\\\\\\\\\\\\\127.0.0.1\\\\\\\\\", \"&1\"]}\nselection_atexec: {\"ParentCommandLine|contains\": [\"svchost.exe -k netsvcs\", \"taskeng.exe\"], \"CommandLine|contains|all\": [\"cmd.exe\", \"/C\", \"Windows\\\\Temp\\\\\", \"&1\"]}",
    "condition": "1 of selection_*",
    "fields": [
      "CommandLine",
      "ParentCommandLine"
    ]
  },
  "falsepositives": [
    "Unknown"
  ],
  "level": "high",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
