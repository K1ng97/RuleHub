  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\regsvr32.exe\"}, {\"OriginalFileName\": \"REGSVR32.EXE\"}]\nselection_flag: {\"CommandLine|contains\": [\" /i\", \" -i\"]}\nselection_protocol: {\"CommandLine|contains\": [\"ftp\", \"http\"]}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
