{
  "id": "f2485272-a156-4773-82d7-1d178bc4905b",
  "name": "Suspicious Service Installed",
  "description": "Detects installation of NalDrv or PROCEXP152 services via registry-keys to non-system32 folders.\nBoth services are used in the tool Ghost-In-The-Logs (https://github.com/bats3c/Ghost-In-The-Logs), which uses KDU (https://github.com/hfiref0x/KDU)\n",
  "source": {
    "type": "sigma",
    "id": "f2485272-a156-4773-82d7-1d178bc4905b",
    "url": "https://github.com/SigmaHQ/sigma/blob/master/registry_set_susp_service_installed.yml",
    "file_path": "tmp/repos/sigma/rules/windows/registry/registry_set/registry_set_susp_service_installed.yml"
  },
  "tags": [
    "attackdefense-evasion",
    "attackt1562001"
  ],
  "author": "xknow (@xknow_infosec), xorxes (@xor_xes)",
  "references": [
    "https://web.archive.org/web/20200419024230/https://blog.dylan.codes/evading-sysmon-and-windows-event-logging/"
  ],
  "severity": "medium",
  "type": "sigma",
  "status": "test",
  "created": "2019-04-08",
  "modified": "2023-08-17",
  "mitre": {
    "tactics": [
      "TA1562"
    ],
    "techniques": [
      "T001"
    ]
  },
  "detection": {
    "query": "selection: {\"TargetObject\": [\"HKLM\\\\System\\\\CurrentControlSet\\\\Services\\\\NalDrv\\\\ImagePath\", \"HKLM\\\\System\\\\CurrentControlSet\\\\Services\\\\PROCEXP152\\\\ImagePath\"]}\nfilter: {\"Image|endswith\": [\"\\\\procexp64.exe\", \"\\\\procexp.exe\", \"\\\\procmon64.exe\", \"\\\\procmon.exe\", \"\\\\handle.exe\", \"\\\\handle64.exe\"], \"Details|contains\": \"\\\\WINDOWS\\\\system32\\\\Drivers\\\\PROCEXP152.SYS\"}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "Other legimate tools using this service names and drivers. Note - clever attackers may easily bypass this detection by just renaming the services. Therefore just Medium-level and don't rely on it."
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "registry_set"
  ]
}