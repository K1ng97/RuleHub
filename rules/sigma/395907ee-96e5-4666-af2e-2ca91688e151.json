  "detection": {
    "query": "selection: {\"Image|endswith\": [\"\\\\wab.exe\", \"\\\\wabmig.exe\"]}\nfilter: {\"Image|startswith\": [\"C:\\\\Windows\\\\WinSxS\\\\\", \"C:\\\\Program Files\\\\Windows Mail\\\\\", \"C:\\\\Program Files (x86)\\\\Windows Mail\\\\\"]}",
    "condition": "selection and not filter",
    "fields": []
  },
  "falsepositives": [
    "Unknown"
  ],
