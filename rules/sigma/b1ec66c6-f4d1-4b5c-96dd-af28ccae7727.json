  "detection": {
    "query": "selection_img: [{\"Image|endswith\": \"\\\\cmdkey.exe\"}, {\"OriginalFileName\": \"cmdkey.exe\"}]\nselection_cli_generic: {\"CommandLine|contains|windash\": \" -g\"}\nselection_cli_user: {\"CommandLine|contains|windash\": \" -u\"}\nselection_cli_password: {\"CommandLine|contains|windash\": \" -p\"}",
    "condition": "all of selection_*",
    "fields": []
  },
  "falsepositives": [
    "Legitimate usage for administration purposes"
  ],
  "level": "medium",
  "rule_format": "standard",
  "platforms": [],
  "data_sources": [
    "windows",
    "process_creation"
  ]
